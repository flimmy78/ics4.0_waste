<?xml version='1.0' encoding='ISO-8859-1'?>
<Application dbconnect="YES" trclog="PNT007.log">
	<VarDef>
		<String alias="RptHead" pos="1"/>
		<String alias="TxnDat"  pos="2"/>
	</VarDef>
	<DataSource object="sql">
		<Iterative>
			<Sql sql="select ZHBankCd,ZHBankNm,TxnDat,TotNum,ZHActNo,ZHActNm,TotAmt,DSBankNo,DSBankNm,DSActNo,TitNum,TitAmt,TCrpCd,TCrpNm,PrjCod,PrjNam,DtlAmt,RevDat  from PntHzfczh where  TxnDat='%s' and dtlsts='1'">              
				<FieldList>$TxnDat|</FieldList>
			</Sql>
		</Iterative>
    <Summed type="sql">
       <Sql sql="select TxnDat,sum(cast(DtlAmt as bigint)) as sumamt,coalesce(COUNT(*),0) SumCnt from PntHzfczh where TxnDat='%s' and dtlsts='1' group by TxnDat">
