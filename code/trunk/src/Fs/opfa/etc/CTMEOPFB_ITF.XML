  <Transaction code="481161" service="481162" desc="佛山代收费抹帐" log_switch="1"> <!--嵌套code="481123" desc="佛山代收通讯费抹帐查询"-->
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
