      <AddItem name="ThdCod"   value="105001"/>
      <AddItem name="AppNam"   value="FS_MOBL" />
      <SwitchCode name="AppNam" new_name="CAgtNo"  tab_name="FS_AppNam"  col_name="AppNam"/>
