	<Transaction code="465825" service="465825" log_switch="1" timeout="60" desc="广东省集中式代收付协议维护">
		<Request>
			<Include file="etc/TRMTIA_IIT.XML"/>
				<Item name="Func"   length="1" /> <!--功能选择-->
				<Switch expression="$Func">
					<Case value="4">	<!--查询-->
						<Item name="AgrNo" length ="55"  expression="DELBOTHSPACE($AgrNo)"/>
						<Item name="UsrCod" length ="18" expression="DELBOTHSPACE($UsrCod)"/>
						<Break/>
					</Case>
          <Default>
						<Item name="AutFlg"     length="1"/>
						<Item name="AgrNo"      length="55"/>
						<Item name="AgrTyp"     length="1"/>
						<Item name="SBusTy"     length="5"/>
						<Item name="SBusCh"     length="20"/>
						<Item name="ActCls"     length="1"/>
						<Item name="CtyCod"     length="4"/>
						<Item name="OrgCod"     length="9" expression="ADDCHAR(DELSPACE($OrgCod,both),9,0,1)"/>
						<Item name="PayCBnk"    length="12"/>
						<Item name="PayCBnkNm"  length="60"/>
						<Item name="PayOBnk"    length="12"/>
						<Item name="PayOBnkNm"  length="60"/>
						<Item name="PayAct"     length="32" expression="DELSPACE($PayAct,both)"/>
						<Item name="PayNam"     length="60"/>
						<Item name="OpnDat"    length="8"/>
						<Item name="UsrCod"     length="18"/>
						<Item name="UsrNam"     length="60"/>
						<Item name="LnkNam"     length="60"/>
						<Item name="LnkTel"     length="25"/>
						<Item name="LnkPst"     length="6"/>
						<Item name="LnkAdr"     length="60"/>
