<?xml version='1.0' encoding='ISO-8859-1'?>
<Application name="CBS" server="OFRTCBS3" pre_dll="dll/pre_term.so" pre_func="pre_term" spe_dll="dll/pre_term.so" spe_func="spe_term" timeout="300">
<!--created by wishman -->
	<Define>
<!--通用回应-->
		<Macro name="COMMON_RES">
			<Switch expression="$MsgTyp">
				<Case value="N">
					<Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
					<Break/>
				</Case>
				<Case value="W"/>
				<Case value="E">
					<PackItem name="TMP">
						<Item name="ApCode" length="2" value="SC"/> 
						<Item name="OFmtCd" length="3" value="D04"/>
						<Item name="RspCod" length="6"/>
						<Item name="InPos"  length="4" value="0001"/>
						<Item name="RspMsg" length="56" />
					</PackItem>
					<Item name="TMP" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Macro>
    <!--查询查复浏览-->
		<Macro name="465833_REQ">
		 <If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx"   length="6"/>
		 </If>
		 <Else>
			<Item name="SndDat" length="8"  expression="DELSPACE($SndDat,both)" /> <!--委托日期-->
      <Item name="OIFlag" length="1" /> <!--往来标志-->
		  <Item name="QryTyp" length="3" /> <!--查询类型-->
      <Item name="MatFlg" length="1" /> <!--查复状态-->
  		<Item name="SOpnBk" length="12" expression="DELSPACE($SOpnBk,both)" /> <!--发起行号-->
	  	<Item name="ROpnBk" length="12" expression="DELSPACE($ROpnBk,both)" /> <!--接收行号-->
      <Item name="ReqSeq" length="8" expression="DELSPACE($ReqSeq,both)" /> <!--查询编号-->
      <Item name="DeaNod" length="6" expression="DELSPACE($DeaNod,both)" /> <!--处理网点-->
      <Item name="BrCd" length="6" expression="DELSPACE($BrCd,both)" />     <!--所属分行-->
		</Else>
		</Macro>
		<Macro name="465833_RES">
		 <If condition="IS_EQUAL_STRING($MsgTyp,N)">
			<PackItem name="TmpDat">
			 <Item name="ApCode"  length="2" value="32"/>
			 <Item name="OFmtCd"  length="3" value="Z01"/>
			 <Item name="PageNo"  length="4" value="0001"/>
			 <Item name="VarSize" length="1" value="3"/>
			 <Item name="Ttl"     head_len="3" value="查询查复浏览"/>
			 <Item name="SubTtl"  head_len="3" value="查询内容"/>
			 <Group name="Rec" repeat_name="RecNum">
				<PackItem name="TmpNam">
  			 <Item name="SndDat" length="8"  /> <!--委托日期   -->
  			 <Item name="QryTyp" length="3"  /> <!--查询标识   -->
  			 <Item name="OIFlag" length="1"  /> <!--往来标志   -->
  			 <Item name="MatFlg" length="1"  /> <!--匹配标识   -->
  			 <Item name="BrNo"   length="6"  /> <!--交易分行   -->
  			 <Item name="SOpnBk" length="12"  /> <!--发起行号   -->
  			 <Item name="ROpnBk" length="12"  /> <!--接收行号   -->
  			 <Item name="ReqSeq" length="8"  /> <!--查询编号   -->
  			 <Item name="RspSeq" length="8"  /> <!--查复编号   -->
  			 <Item name="DeaNod" length="6"  /> <!--处理网点   -->
  			 <Item name="DeaTlr" length="7"  /> <!--处理柜员   -->
  			 <Item name="DeaDat" length="8"  /> <!--处理时间   -->
  			 <Item name="PTraKnd" length="4"  /> <!--原交易类型 -->
  			 <Item name="PSndDat" length="8"  /> <!--原委托日期 -->
  			 <Item name="PTxnAmt" length="15"  /> <!--原交易金额 -->
  			 <Item name="PCcyCod" length="3"  /> <!--原交易币种 -->
  			 <Item name="PVchId" length="8"  /> <!--原凭证号   -->
  			 <Item name="PPayAct" length="35"  /> <!--原付款帐号 -->
  			 <Item name="PPayNam" length="60"  /> <!--原付款名称 -->
  			 <Item name="PGatAct" length="35"  /> <!--原收款帐号 -->
  			 <Item name="PGatNam" length="60"  /> <!--原收款名称 -->
  			 <Item name="ReqSmr" length="120"  /> <!--查询备注   -->
  			 <Item name="RspSmr" length="120"  /> <!--回复备注   -->
				</PackItem>
				<Item name="TmpNam"   head_len="3"/>
		   </Group>
			</PackItem>
			<Item name="TmpDat" head_len="4" include_len="yes"/>
		 </If>
		 <Else>
			<Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
		 </Else>
		</Macro>

    <!--对帐汇总信息浏览-->
		<Macro name="465835_REQ">
		 <If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx"   length="6"/>
		 </If>
		 <Else>
			<Item name="CTraKnd" length="4"  expression="DELSPACE($CTraKnd,both)"/> <!--交易类型-->
			<Item name="DBrCd"   length="12" expression="DELSPACE($DBrCd,both)"  /> <!--清算行号-->
			<Item name="ClrDat"  length="8"  expression="DELSPACE($ClrDat,both)" /> <!--清算日期-->
			<Item name="AreaNo"  length="4"  expression="DELSPACE($AreaNo,both)" /> <!--地区代码-->
			<Item name="CcyCod"  length="3"  expression="DELSPACE($CcyCod,both)" /> <!--币种-->
		</Else>
		</Macro>
		<Macro name="465835_RES">
		 <If condition="IS_EQUAL_STRING($MsgTyp,N)">
			<PackItem name="TmpDat">
			 <Item name="ApCode"  length="2" value="32"/>
			 <Item name="OFmtCd"  length="3" value="Z01"/>
			 <Item name="PageNo"  length="4" value="0001"/>
			 <Item name="VarSize" length="1" value="3"/>
			 <Item name="Ttl"     head_len="3" value="对帐汇总信息浏览"/>
			 <Item name="SubTtl"  head_len="3" value="查询内容"/>
			 <Group name="Rec" repeat_name="RecNum">
			  <PackItem name="TmpNam">
			   <Item name="CTraKnd" length="4"  />  <!--交易类型-->
			   <Item name="TraTm"   length="10" />  <!--交易时间-->
			   <Item name="DBrCd"   length="12" />  <!--清算行号-->
			   <Item name="ClrDat"  length="8"  />  <!--清算日期-->
			   <Item name="AreaNo"  length="4"  />  <!--地区代码-->
			   <Item name="CcyCod"  length="3"  />  <!--币种-->
			   <Item name="ChkFlg"  length="1"  />  <!--对帐标识-->
			   <Item name="CreAmt"  length="15"  /> <!--人行贷方金额-->
			   <Item name="CreCnt"  length="10"  /> <!--人行贷方笔数-->
			   <Item name="DebAmt"  length="15"  /> <!--人行借方金额-->
			   <Item name="DebCnt"  length="10"  /> <!--人行借方笔数-->
			   <Item name="CreAmt2" length="15"  /> <!--银行贷方金额-->
			   <Item name="CreCnt2" length="10"  /> <!--银行贷方笔数-->
			   <Item name="DebAmt2" length="15"  /> <!--银行借方金额-->
			   <Item name="DebCnt2" length="10"  /> <!--银行借方笔数-->
			  </PackItem>
			  <Item name="TmpNam"   head_len="3"/>
		   </Group>
			</PackItem>
			<Item name="TmpDat" head_len="4" include_len="yes"/>
		 </If>
		 <Else>
			<Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
		 </Else>
		</Macro>

    <!--汇兑统计监控信息-->
		<Macro name="465836_REQ">
		 <If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx"   length="6"/>
		 </If>
		 <Else>
			<Item name="TxnDat"  length="8"  expression="DELSPACE($TxnDat,both)" /> <!--交易会计日期-->
 		  <Item name="CcyCod"  length="3"  expression="DELSPACE($CcyCod,both)" /> <!--币种-->
      <Item name="BrCd"   length="6"  /> <!--交易分行   -->
		</Else>
		</Macro>
		<Macro name="465836_RES">
		 <If condition="IS_EQUAL_STRING($MsgTyp,N)">
			<PackItem name="TmpDat">
			 <Item name="ApCode"  length="2" value="32"/>
			 <Item name="OFmtCd"  length="3" value="Z01"/>
			 <Item name="PageNo"  length="4" value="0001"/>
			 <Item name="VarSize" length="1" value="3"/>
			 <Item name="Ttl"     head_len="3" value="汇兑统计监控信息浏览"/>
			 <Item name="SubTtl"  head_len="3" value="查询内容"/>
			 <Group name="Rec" repeat_name="RecNum">
			  <PackItem name="TmpNam">
			  <Item name="ActDat"  length="8"  /> <!--交易日期-->
  			<Item name="OIFlag"  length="1"  /> <!--往来标志 -->
				<Item name="BilSts"  length="1"  /> <!--交易状态 -->
		    <Item name="TotCnt"  length="10" /> <!--统计笔数-->
			  <Item name="TotAmt"  length="15" align_mode="right" fill_asc="48"/> <!--统计金额-->
			  </PackItem>
			  <Item name="TmpNam"   head_len="3"/>
		   </Group>
			</PackItem>
			<Item name="TmpDat" head_len="4" include_len="yes"/>
		 </If>
		 <Else>
			<Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
		 </Else>
		</Macro>

    <!--日终汇总对帐-->
		<Macro name="465706_REQ">
			<Item name="ClrDat" length="8"  /> <!--清算日期-->
			<Item name="DBrCd"  length="12"  /> <!--直接清算行-->
			<Item name="AreaNo" length="4"  /> <!--开通区域-->
		</Macro>
		<Macro name="465706_RES">
		 <If condition="IS_EQUAL_STRING($MsgTyp,N)">
      <PackItem name="TmpDat">
       <Item name="ApCode" length="2" value="00"/>
       <Item name="OFmtCd" length="3" value="000"/>
       <Item name="RspMsg" length="60"  /> <!--回应信息-->
      </PackItem>
      <Item name="TmpDat" head_len="4" include_len="yes"/>
		 </If>
			<Quote name="COMMON_RES"/>
		</Macro>

    <!--日终人行明细数据下载-->
		<Macro name="465708_REQ">
			<Item name="ClrDat" length="8"/> <!--清算日期-->
			<Item name="CTraKnd" length="4"/> <!--选择交易种类-->
		</Macro>
		<Macro name="465708_RES">
			<Quote name="COMMON_RES"/>
		</Macro>

    <!--分行日终清算汇总打印-->
		<Macro name="465711_REQ">
			<Item name="ClrDat" length="8"  /> <!--清算日期-->
			<Item name="DBrCd"  length="12"  /> <!--直接清算行-->
			<Item name="AreaNo" length="4"  /> <!--开通区域-->
		</Macro>
		<Macro name="465711_RES">
		 <If condition="IS_EQUAL_STRING($MsgTyp,N)">
      <PackItem name="TmpDat">
       <Item name="ApCode" length="2" value="00"/>
       <Item name="OFmtCd" length="3" value="000"/>
       <Item name="RspMsg" length="60"  /> <!--回应信息-->
      </PackItem>
      <Item name="TmpDat" head_len="4" include_len="yes"/>
		 </If>
			<Quote name="COMMON_RES"/>
		</Macro>
	</Define>

	<Transaction code="465833" service="465833" timeout="30" log_switch="1" desc="查询查复浏览" >
		<Request>
			<Include file="etc/TRMTIA_IIT.XML"/>
			<Quote name="465833_REQ"/>
		</Request>
		<Response necessary="no">
			<Include file="etc/TRMTOA_IIT.XML"/>
			<Quote name="465833_RES"/>
			<Quote name="COMMON_RES"/>		
		</Response>
	</Transaction>

  <Transaction code="465834" service="465834" log_switch="1" timeout="30" desc="查询查复多笔浏览">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
			<If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx" length="6"/>
			</If>
			<Else>
				<Item name="Func"   length="1" /> <!--功能选择-->
				<Switch expression="$Func">
					<Case value="1">	<!--查询-->
			      <Item name="SndDat" length="8"  expression="DELSPACE($SndDat,both)" /> <!--委托日期-->
            <Item name="OIFlag" length="1" /> <!--往来标志-->
		        <Item name="QryTyp" length="3" /> <!--查询类型-->
            <Item name="MatFlg" length="1" /> <!--查复状态-->
  		      <Item name="SOpnBk" length="12" expression="DELSPACE($SOpnBk,both)" /> <!--发起行号-->
	  	      <Item name="ROpnBk" length="12" expression="DELSPACE($ROpnBk,both)" /> <!--接收行号-->
            <Item name="ReqSeq" length="8" expression="DELSPACE($ReqSeq,both)" /> <!--查询编号-->
            <Item name="DeaNod" length="6" expression="DELSPACE($DeaNod,both)" /> <!--处理网点-->
            <Item name="BrCd" length="6" expression="DELSPACE($BrCd,both)" />     <!--所属分行-->
						<Break/>
					</Case>
					<Case value="2">	<!--处理-->
			      <Item name="SndDat" length="8"  expression="DELSPACE($SndDat,both)" /> <!--委托日期-->
            <Item name="OIFlag" length="1" /> <!--往来标志-->
            <Item name="SndBNo" length="12" /> <!--查询发起行-->
            <Item name="MsgSeq" length="16" /> <!--信息序号-->
						<Break/>
					</Case>
				</Switch>
			</Else>
    </Request>
    <Response necessary="no">
      <Include file="etc/TRMTOA_IIT.XML"/>
			<Switch expression="$Func">
				<Case value="1">
					<If condition="IS_EQUAL_STRING($MsgTyp,N)">						
						<PackItem name="TmpDat">
								<Item name="ApCode"  length="2" value="32"/>
								<Item name="OFmtCd"  length="3" value="Z01"/>
								<Item name="PageNo"  length="4" value="0001"/>
								<Item name="VarSize" length="1" value="3"/>
								<Item name="Ttl"     head_len="3" value="批注浏览"/>
								<Item name="SubTtl"  head_len="3" value="查询内容"/>
							<Group name="Rec" repeat_name="RecNum">
								<PackItem name="TmpNam">
  			          <Item name="SndDat" length="8"  /> <!--委托日期   -->
  			          <Item name="QryTyp" length="3"  /> <!--查询标识   -->
  			          <Item name="OIFlag" length="1"  /> <!--往来标志   -->
  			          <Item name="MatFlg" length="1"  /> <!--匹配标识   -->
  			          <Item name="BrNo"   length="6"  /> <!--交易分行   -->
  			          <Item name="SOpnBk" length="12"  /> <!--发起行号   -->
  			          <Item name="ROpnBk" length="12"  /> <!--接收行号   -->
  			          <Item name="ReqSeq" length="8"  /> <!--查询编号   -->
  			          <Item name="RspSeq" length="8"  /> <!--查复编号   -->
  			          <Item name="DeaNod" length="6"  /> <!--处理网点   -->
  			          <Item name="DeaTlr" length="7"  /> <!--处理柜员   -->
  			          <Item name="DeaDat" length="8"  /> <!--处理时间   -->
  			          <Item name="PTraKnd" length="4"  /> <!--原交易类型 -->
  			          <Item name="PSndDat" length="8"  /> <!--原委托日期 -->
  			          <Item name="PTxnAmt" length="15"  /> <!--原交易金额 -->
  			          <Item name="PCcyCod" length="3"  /> <!--原交易币种 -->
  			          <Item name="PVchId" length="8"  /> <!--原凭证号   -->
  			          <Item name="PPayAct" length="35"  /> <!--原付款帐号 -->
  			          <Item name="PPayNam" length="60"  /> <!--原付款名称 -->
  			          <Item name="PGatAct" length="35"  /> <!--原收款帐号 -->
  			          <Item name="PGatNam" length="60"  /> <!--原收款名称 -->
                  <Item name="SndBNo" length="12" /> <!--查询发起行-->
                  <Item name="MsgSeq" length="16" /> <!--信息序号-->
								</PackItem>
								<Item name="TmpNam"   head_len="3"/>
							</Group>
						</PackItem>
						<Item name="TmpDat" head_len="4" include_len="yes"/>
					</If>
					<Break/>
				</Case>	
				<Case value="2">
					<If condition="IS_EQUAL_STRING($MsgTyp,N)">						
							<PackItem name="TmpDat">
								<Item name="ApCode" length="2" value="46"/>
								<Item name="OFmtCd" length="3" value="999"/>
  			        <Item name="SndDat" length="8"  /> <!--委托日期   -->
  			        <Item name="QryTyp" length="3"  /> <!--查询标识   -->
  			        <Item name="OIFlag" length="1"  /> <!--往来标志   -->
  			        <Item name="MatFlg" length="1"  /> <!--匹配标识   -->
  			        <Item name="BrNo"   length="6"  /> <!--交易分行   -->
  			        <Item name="SOpnBk" length="12"  /> <!--发起行号   -->
  			        <Item name="ROpnBk" length="12"  /> <!--接收行号   -->
  			        <Item name="ReqSeq" length="8"  /> <!--查询编号   -->
  			        <Item name="RspSeq" length="8"  /> <!--查复编号   -->
  			        <Item name="DeaNod" length="6"  /> <!--处理网点   -->
  			        <Item name="DeaTlr" length="7"  /> <!--处理柜员   -->
  			        <Item name="DeaDat" length="8"  /> <!--处理时间   -->
  			        <Item name="PTraKnd" length="4"  /> <!--原交易类型 -->
  			        <Item name="PSndDat" length="8"  /> <!--原委托日期 -->
  			        <Item name="PTxnAmt" length="15"  /> <!--原交易金额 -->
  			        <Item name="PCcyCod" length="3"  /> <!--原交易币种 -->
  			        <Item name="PVchId"  length="8"  /> <!--原凭证号   -->
  			        <Item name="PPayAct" length="35"  /> <!--原付款帐号 -->
  			        <Item name="PPayNam" length="60"  /> <!--原付款名称 -->
  			        <Item name="PGatAct" length="35"  /> <!--原收款帐号 -->
  			        <Item name="PGatNam" length="60"  /> <!--原收款名称 -->
  			        <Item name="ReqSmr"  length="120" expression="TOSBC($ReqSmr)" /> <!--查询备注   -->
  			        <Item name="RspSmr"  length="120"  /> <!--回复备注   -->
							</PackItem>
							<Item name="TmpDat" head_len="4" include_len="yes"/>
					</If>
					<Break/>
				</Case>																
			</Switch>
  		<Quote name="COMMON_RES"/>		
    </Response>
  </Transaction>

	<Transaction code="465835" service="465835" timeout="30" log_switch="0" desc="对帐汇总信息浏览" >
