			<Item name="BilAmt" deli_asc="124" expression="DELSPACE($BilAmt,both)" desc="退费金额"/>
			<Item name="FulCrc" deli_asc="124" expression="DELSPACE($FulCrc,both)" desc="全书校验码"/>
			<Item name="PayDat" deli_asc="124" expression="DELSPACE($PayDat,both)" desc="退费日期"/>
