	<Transaction code="315" log_switch="1" desc="更换票据">
		<Request>
			<Item name="CmdCod" value="315" deli_asc="124" desc="命令字"/>
			<Item name="BankCd" deli_asc="124" expression="DELSPACE($BankCd,both)" desc="银行编码"/>
			<Item name="TCrpCd" deli_asc="124" expression="DELSPACE($TCrpCd,both)" desc="执收单位编码"/>
			<Item name="TVchId" deli_asc="124" expression="DELSPACE($TVchId,both)" desc="缴款通知书编码"/>
			<Item name="OTBilTp" deli_asc="124" expression="DELSPACE($OTBilTp,both)" desc="原票据类型编码"/>
			<Item name="OTBilNo" deli_asc="124" expression="DELSPACE($OTBilNo,both)" desc="原票据号码"/>
			<Item name="NTBilTp"  deli_asc="124" expression="DELSPACE($NTBilTp,both)" desc="现票据类型编码"/>
			<Item name="NTBilNo" deli_asc="124" expression="DELSPACE($NTBilNo,both)" desc="现票据号码"/>
			<Item name="Smr" deli_asc="124" expression="DELSPACE($Smr,both)" desc="更换原因"/>
		</Request>
		<Response necessary="no">
