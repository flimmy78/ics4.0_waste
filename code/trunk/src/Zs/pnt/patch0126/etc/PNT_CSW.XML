		<Item RtnCod="252" RspMsg="全书校验码校验失败"/>
		<Item RtnCod="261" RspMsg="实收金额错误"/>
		<Item RtnCod="262" RspMsg="实收滞纳金错误"/>
		<Item RtnCod="271" RspMsg="用错命令字执行缴费"/>
