<?xml version='1.0' encoding='ISO-8859-1'?>
<Root>
	<Table name="RtnCodToTRspCd" desc="第三方返回码转换">
		<Item RtnCod="100" TRspCd="000000" desc="交易成功"/>
		<Item RtnCod="102" TRspCd="PNT102" desc="提示信息，需要部分手工录入通知书"/>
		<Item RtnCod="107" TRspCd="PNT107" desc="提示信息，需要完全手工录入通知书"/>
		<Item RtnCod="210" TRspCd="PNT210" desc="数据包或数据格式错误，例如空白数据包、未知命令字、数据包字段数目不对等"/>
		<Item RtnCod="211" TRspCd="PNT211" desc="禁止访问，即进行的数据操作在非税系统中是被禁止的，例如执收单位已被禁用，收费项目已被禁用，票据类型已被禁用等"/>
		<Item RtnCod="219" TRspCd="PNT219" desc="其他错误，表示未定义返回码的逻辑错误"/>
		<Item RtnCod="299" TRspCd="PNT299" desc="系统错误"/>
		<Item RtnCod="231" TRspCd="PNT231" desc="该缴款通知书号码在系统中不存在"/>
		<Item RtnCod="232" TRspCd="PNT232" desc="该缴款通知书已经被缴费"/>
		<Item RtnCod="233" TRspCd="PNT233" desc="该缴款通知书已经被取消"/>
		<Item RtnCod="234" TRspCd="PNT234" desc="该缴款通知书状态不正确"/>
		<Item RtnCod="235" TRspCd="PNT235" desc="该缴款通知书已经过了截止日期"/>
		<Item RtnCod="236" TRspCd="PNT236" desc="该缴款通知书不应在此银行缴费"/>
		<Item RtnCod="241" TRspCd="PNT241" desc="该票据号码在系统中不存在"/>
		<Item RtnCod="242" TRspCd="PNT242" desc="该票据已经被使用"/>
		<Item RtnCod="243" TRspCd="PNT243" desc="该票据已经被作废"/>
		<Item RtnCod="244" TRspCd="PNT244" desc="该票据状态不正确"/>
		<Item RtnCod="246" TRspCd="PNT246" desc="该票据不属于此银行"/>
		<Item RtnCod="247" TRspCd="PNT247" desc="该缴款通知书不应开出此种类型的票据"/>
		<Item RtnCod="248" TRspCd="PNT248" desc="该缴款通知书和该票据不对应"/>
		<Item RtnCod="251" TRspCd="PNT251" desc="号码校验码校验失败"/>
		<Item RtnCod="252" TRspCd="PNT252" desc="全书校验码校验失败"/>
		<Item RtnCod="261" TRspCd="PNT261" desc="实收金额错误"/>
		<Item RtnCod="262" TRspCd="PNT262" desc="实收滞纳金错误"/>
		<Item RtnCod="271" TRspCd="PNT271" desc="用错命令字执行缴费"/>
		<Default TRspCd="PNT999"/>
	</Table>
	<Table name="RtnCodToRspMsg" desc="第三方返回码中文转换">
		<Item RtnCod="100" RspMsg="操作成功"/>
		<Item RtnCod="102" RspMsg="提示信息，需要部分手工录入通知书"/>
