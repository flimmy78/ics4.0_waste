<?xml version='1.0' encoding='ISO-8859-1'?>
<Application trclog="EFE_RPT_CFG.trc">
  <VarDef>
  	<String alias="BrNo"    pos="1"/>
    <String alias="CAgtNo"  pos="2"/>
    <String alias="TxnDat1"  pos="3"/>
    <String alias="TxnDat2"  pos="4"/>
    <String alias="PNodNo"  pos="5"/>
    <String alias="PTlrId"  pos="6"/>
  </VarDef>
  <MsgConvert>
    <Item alias="TxnCod">
      <Convert value="460205" msg="银行柜台代收"/>
      <Convert value="460220" msg="供电单笔代扣"/>
      <Convert value="460200" msg="供电批量代扣"/>
    </Item>
    <Item alias="BrNo">
      <Convert value="441999" msg="广东分行"/>
      <Convert value="445999" msg="汕头分行"/>
      <Convert value="446999" msg="佛山分行"/>
      <Convert value="491999" msg="惠州分行"/>
      <Convert value="444999" msg="珠海分行"/>
      <Convert value="484999" msg="中山分行"/>
      <Convert value="483999" msg="东莞分行"/>
      <Convert value="485999" msg="揭阳分行"/>
      <Convert value="761999" msg="江门分行"/>
      <DEFAULT msg="未知"/>
    </Item>
  </MsgConvert>
 <DataSource object="sql">
    <Iterative>
      <Sql sql="SELECT a.*,b.logno,b.dept
				FROM afetxnjnl a,efebatinf b WHERE a.ActDat &gt;='%s' and a.ActDat &lt;='%s' and a.CAgtNo='%s' and a.HTxnSt!='S' and a.Logno=b.Logno order by b.dept,a.TxnCod">
				<FieldList>$TxnDat1|$TxnDat2|$CAgtNo|</FieldList>
	   </Sql>
    </Iterative>
    <Summed type="sql">
       <Sql sql="select sum(cast(a.TxnAmt as bigint)) as sumamt,coalesce(COUNT(*),0) TotCnt from afetxnjnl a,efebatinf b where a.logno=b.logno and a.ActDat &gt;='%s' and a.ActDat &lt;='%s' and  a.CAgtNo= '%s' and a.HTxnSt!='S'">
       <FieldList>$TxnDat1|$TxnDat2|$CAgtNo|</FieldList>
	   </Sql>
    </Summed>
  </DataSource>
  <FormatDefine pagerows="41" pagesum="YES">
    <TableHeader line="1">
    	 <Para label="01"  alias="$BrNo"   convert="YES" spread="right"></Para>
       <Para label="02"  alias="电力电费业务" spread="right" ></Para>
       <Para label="03"  alias="SUBSTR($TxnDat1,1,4)" spread="right" ></Para>
       <Para label="04"  alias="SUBSTR($TxnDat1,5,2)" spread="right"></Para>
       <Para label="05"  alias="SUBSTR($TxnDat1,7,2)" spread="right"></Para>
       <Para label="06"  alias="SUBSTR($TxnDat2,1,4)" spread="right" ></Para>
       <Para label="07"  alias="SUBSTR($TxnDat2,5,2)" spread="right"></Para>
       <Para label="08"  alias="SUBSTR($TxnDat2,7,2)" spread="right"></Para>
       <Para label="09"  alias="$PAGESUM" spread="mid"></Para>
       <Para label="10"  alias="$PAGENUM" spread="mid"></Para>
       <Format>
                                   #代理业务缴费批扣可疑明细表

        业务类型:#             日期:  #年  #月  #日至 #年  #月  #日    共 # 页    第 # 页
┏━━━━━━━━━┳━━━━━━━━━┳━━━━━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┳━━━━━━━┓
┃  交易日期        ┃     缴费项目     ┃          帐号        ┃  前置流水号  ┃  会计流水号  ┃    金额      ┃供电收款单位  ┃
┣━━━━━━━━━╋━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━╋━━━━━━━┫
       </Format>
    </TableHeader>
    <Iterative line="1">       
       <Para label="01" alias="SUBSTR($FTXNTM,1,8)" spread="right"></Para>
       <Para label="02" alias="$TxnCod" convert="YES" spread="right"></Para>
       <Para label="03" alias="$ActNo"  spread="right"></Para>
       <Para label="04" alias="DELBOTHSPACE($LogNo)"  spread="right"></Para>
       <Para label="05" alias="DELBOTHSPACE($TckNo)"  spread="right"></Para>
       <Para label="06" alias="AMTFMT($TxnAmt)" spread="left"></Para>   
       <Para label="07" alias="$DEPT"   spread="right" ></Para>
       <Format>
┃#                 ┃#                 ┃#                     ┃#             ┃#             ┃             #┃#             ┃
       </Format>
    </Iterative>
    <Summed line='1'>
       <Para label="01" alias="$TotCnt"         spread="left"></Para>
       <Para label="02" alias="AMTFMT($sumamt)" spread="left"></Para>
       <Format>
┣━━━━━━━━━┻━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━┻━━━━━━━┻━━━━━━━┻━━━━━━━┫
┃合计                                   交易可疑笔数:   #                            交易可疑金额：            #               ┃
     </Format>
    </Summed>
    <TableFooter line="1">
       <Para label="01" alias="$PNodNo" spread="right"></Para>
       <Para label="02" alias="$PTlrId" spread="right"></Para>
       <Para label="03" alias="GETDATETIME(YYYY-MM-DD)" spread="left"></Para>
       <Format>
┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
	                  打印部门：#           打印柜员：#          打印日期：#
       </Format>
    </TableFooter>
  </FormatDefine>
</Application>
