<?xml version='1.0' encoding='ISO-8859-1'?>
<ComuConfig>
   <Global>
      <ConnTyp>0</ConnTyp><!--连接类型, 必填-->
      <MaxConn>50</MaxConn><!--最大连接数, 默认为50-->
      <IpLstFil>etc/CTHDEFEA_IPD.XML</IpLstFil><!--允许连接IP 列表文件名-->
      <TmOut>30</TmOut><!--超时时间, 默认为30 秒-->
   </Global>
   <MsgHead>
      <Receive>
         <MsgHeadLen length="0" save="no"/><!--包头长度-->
         <MsgBodyLen startpos="0" length="4" value_type="STRING" is_include_headlen="no"/>
      </Receive>
      <Send>
         <MsgHeadLen>0</MsgHeadLen>
      </Send>
   </MsgHead>
</ComuConfig>

