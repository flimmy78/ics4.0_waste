            <Exec func="PUB:Unlock">
              <Arg name="RecKey" value="$RecKey"/>
            </Exec>
            <Set>MsgTyp=E</Set>
            <Set>RspCod=EFE999</Set>
            <Set>RspMsg=STRCAT(更新单面编码【,$DWBM,】明细记录出错)</Set>
            <Return/>
          </If-->
          <Exec func="PUB:CommitWork"/>
        </If>
        <Else>
          <Exec func="PUB:CloseCursor" error="IGNORE"/>
          <Exec func="PUB:Unlock">
            <Arg name="RecKey" value="$RecKey"/>
          </Exec>
          <Set>MsgTyp=E</Set>
          <Set>RspCod=EFE999</Set>
          <Set>RspMsg=发送电网通讯异常</Set>
          <Return/>
        </Else>
