        select distinct SUBSTR(DWBM,1,6) DWBM from EfekCusAgt where FQFJYRQ='%s'
      </Sentence>
      <Fields>QryDat|</Fields>
    </DynSentence>
     <DynSentence name="CntCusAgt" desc="统计数量">
