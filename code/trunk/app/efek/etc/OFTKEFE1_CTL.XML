        <Set>ZKHS=$MXBS</Set>
        <Set>YHWJMC=STRCAT(HDXY0301,$DWBM,$QRYDAT,001.txt)</Set>
        <Set>WJLX=04</Set>
        <Set>DatFil=STRCAT(GETENV(WORKDIR),$EfekSndDir,$YHWJMC)</Set>
        <Exec func="PUB:ExportFromDB">
          <Args>
            <Arg name="FormatName" value="HDXY"/>
