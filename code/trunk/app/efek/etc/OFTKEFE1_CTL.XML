        <Set>WJMD5=TRIM($WJMD5,right)</Set>
        <Exec func="PUB:CloseFile"/>
        <Set>SvrCod=460431</Set>
        <Set>PKGCNT=000001</Set>
        <Quote name="SendThirdOther"/>
        <If condition="INTCMP(~RetCod,3,0)">
          <!--Exec func="PUB:ExecSql" error="IGNORE">
            <Args>
