      <AddItem name="KKSJ" expression="$FQFJYSJ" desc="存入时间"/>
      <Item name="KKRQ"   length=" 8" desc="扣款日期"/>
      <Item name="KKSJ"   length=" 6" desc="扣款时间"/>
    </Request>
    <Response necessary="no">
      <Quote name="SndRspHead"/>
