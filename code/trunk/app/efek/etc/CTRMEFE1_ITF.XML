            <Item name="TLogNo" length="16"/>   <!--流水            -->
            <Item name="ODT"  length="8   "/>   <!--原委托日期      -->
            <Item name="OCC"  length="3   "/>   <!--原指令代码      -->
            <Item name="OCN"  length="12  "/>   <!--原委托节点代码  -->
