              <Item name="MNS"   length="2" />  <!-- 次数          -->
              <Item name="MON"   length="6" />  <!-- 月份          -->
              <Item name="PCC"   length="12"/>  <!-- 违约金        -->
              <Item name="CPS"   length="12"/>  <!-- 本金          -->
              <Item name="DNLN"  length="8" />  <!-- 电量          -->
