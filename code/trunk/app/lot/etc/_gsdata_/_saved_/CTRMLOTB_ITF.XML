<?xml version='1.0' encoding='ISO-8859-1'?>
<Application name="LOT"  pre_dll="dll/pre_term.so" pre_func="pre_term" spe_dll="dll/pre_term.so" spe_func="spe_term" server="OFRTLOTA">
  
	<Transaction code="485412" service="485412" server="OFRTLOTB" log_switch="6" desc="实时投注">
		<Request necessary="no">
			<Include file="etc/TRMTIA_IIT.XML"/>
			<Item name="BetTyp"   length="1"   expression="TRIM($BetTyp,both)" desc="投注类型"/>
			<Item name="SchTyp"   length="1"   expression="TRIM($SchTyp,both)" desc="方案类型"/>
			<Item name="SecLev"   length="1"   expression="TRIM($SecLev,both)" desc="方案等级"/>
			<Item name="CrdNo"    length="21"  expression="TRIM($CrdNo,both)"  desc="客户银行卡号"/>
			<Item name="GameId"   length="2"   expression="TRIM($GameId,both)" desc="游戏编号"/>
			<Item name="PlayId"   length="5"   expression="TRIM($PlayId,both)" desc="玩法编号"/>
			<Item name="DrawId"   length="5"   expression="TRIM($DrawId,both)" desc="当前大期"/>
			<Item name="KenoId"   length="5"   expression="TRIM($KenoId,both)" desc="当前小期"/>
			<Item name="BetMod"   length="5"   expression="TRIM($BetMod,both)" desc="投注方式"/>
			<Item name="BetMet"   length="5"   expression="TRIM($BetMet,both)" desc="投注方法"/>
			<Item name="BetMul"   length="3"   expression="TRIM($BetMul,both)" desc="投注倍数"/>
			<Item name="BetAmt"   length="15"  expression="TRIM($BetAmt,both)" desc="投注金额"/>
			<Item name="BetLin"   length="128" expression="TRIM($BetLin,both)" desc="投注号码"/>
		</Request>
		<Response necessary="no">
			<Include file="etc/TRMTOA_IIT.XML"/>
			<Switch expression="$MsgTyp">
				<Case value="N">
					<PackItem name="TmpDat">
						<Item name="ApFmt"  length="5" value="48000" desc="前端空格式码"/>
						<Item name="TLogNo" length="15" desc="彩票流水号"/>
						<Item name="Cipher" length="30" desc="彩票密码"/>
						<Item name="Verify" length="30" desc="彩票校验码"/>
						<Item name="LotNam" length="30" desc="彩民标识"/>
						<Item name="LotBal" length="18" desc="彩民余额"/>
					</PackItem>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="E">
					<Include file="etc/ERRFMT_IIT.XML"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Response>
	</Transaction>

	<Transaction code="485413" service="485413" server="OFRTLOTB" log_switch="6" desc="投注查询">
		<Request necessary="no">
			<Include file="etc/TRMTIA_IIT.XML"/>
			<Item name="CrdNo"    length="21" expression="TRIM($CrdNo,both)"   desc="客户银行卡号"/>
			<Item name="BetTyp"   length="1"  expression="TRIM($BetLin,both)"  desc="投注类型 0:实时投注；1：定投投注"/>
			<Item name="BegDat"   length="8"  expression="TRIM($BegDat,both)"  desc="起始日期"/>
			<Item name="EndDat"   length="8"  expression="TRIM($EndDat,both)"  desc="结束日期"/>
		</Request>
		<Response necessary="no">
			<Include file="etc/TRMTOA_IIT.XML"/>
			<Switch expression="$MsgTyp">
				<Case value="N">
					<PackItem name="TmpDat">
						<Item name="ApCode"  length="2" value="32"/>
						<Item name="OFmtCd"  length="3" value="Z01"/>
						<Item name="PageNo"  length="4" value="0001"/>
						<Item name="VarSize" length="1" value="3"/>
						<Item name="Ttl"     head_len="3" value="浏览"/>
						<Item name="SubTtl"  head_len="3" value="查询内容"/>
						<Group name="Rec">
							<PackItem name="TmpNam">
								<Item name="GameId"   length="2"   desc="游戏编号"/>
								<Item name="PlayId"   length="5"   desc="玩法编号"/>
								<Item name="TLogNo"   length="15"  desc="彩票流水号"/>
								<Item name="DrawId"   length="5"   desc="当前大期"/>
								<Item name="KenoId"   length="5"   desc="当前小期"/>
								<Item name="BetMul"   length="3"   desc="投注倍数"/>
								<Item name="BetAmt"   length="15"  desc="投注金额"/>
								<Item name="BetLin"   length="128" desc="投注号码"/>
							</PackItem>
							<Item name="TmpNam" head_len="3"/>
						</Group>
					</PackItem>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="E">
					<Include file="etc/ERRFMT_IIT.XML"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Response>
	</Transaction>
	
	<Transaction code="485414" service="485414" server="OFRTLOTB" log_switch="6" desc="中奖查询">
		<Request necessary="no">
			<Include file="etc/TRMTIA_IIT.XML"/>
			<Item name="CrdNo"    length="21"  expression="TRIM($CrdNo,both)"  desc="客户银行卡号"/>
			<Item name="BetTyp"   length="1"   expression="TRIM($BetLin,both)" desc="投注类型 0:实时投注；1：定投投注；空：全部"/>
			<Item name="BegDat"   length="8"  expression="TRIM($BegDat,both)"  desc="起始日期"/>
			<Item name="EndDat"   length="8"  expression="TRIM($EndDat,both)"  desc="结束日期"/>
		</Request>
		<Response necessary="no">
			<Include file="etc/TRMTOA_IIT.XML"/>
			<Switch expression="$MsgTyp">
				<Case value="N">
					<PackItem name="TmpDat">
						<Item name="ApCode"  length="2" value="32"/>
						<Item name="OFmtCd"  length="3" value="Z01"/>
						<Item name="PageNo"  length="4" value="0001"/>
						<Item name="VarSize" length="1" value="3"/>
						<Item name="Ttl"     head_len="3" value="浏览"/>
						<Item name="SubTtl"  head_len="3" value="查询内容"/>
						<Group name="Rec">
							<PackItem name="TmpNam">
								<Item name="TLogNo"   length="15"  desc="彩票流水号"/>
								<Item name="DrawId"   length="5"   desc="当前大期"/>
								<Item name="KenoId"   length="5"   desc="当前小期"/>
								<Item name="BetMul"   length="3"   desc="投注倍数"/>
								<Item name="BetLin"   length="128" desc="投注号码"/>
								<Item name="BetMod"   length="5"   desc="投注方式  1单式，12复式，13胆托"/>
								<Item name="PrzAmt"   length="15"  desc="返奖金额"/>
							</PackItem>
							<Item name="TmpNam" head_len="3"/>
						</Group>
					</PackItem>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="E">
					<Include file="etc/ERRFMT_IIT.XML"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Response>
	</Transaction>

</Application>