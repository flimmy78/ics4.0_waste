	<Transaction code="485415" service="485415" server="OFRTLOTB" log_switch="6" desc="实时投注查询">
		<Request necessary="no">
			<Include file="etc/TRMTIA_IIT.XML"/>
			<Item name="CrdNo"    length="21"   expression="TRIM($CrdNo,both)"  desc="客户银行卡号"/>
			<Item name="TxnLog"   length="32"   expression="TRIM($TxnLog,both)" desc="投注交易流水号"/>
		</Request>
