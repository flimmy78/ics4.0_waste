		<Response necessary="no">
			<Include file="etc/TRMTOA_IIT.XML"/>
			<Switch expression="$MsgTyp">
				<Case value="N">
					<PackItem name="TmpDat">
						<Item name="ApCode"  length="2" value="32"/>
						<Item name="OFmtCd"  length="3" value="Z01"/>
						<Item name="PageNo"  length="4" value="0001"/>
						<Item name="VarSize" length="1" value="3"/>
						<Item name="Ttl"     head_len="3" value="浏览"/>
						<Item name="SubTtl"  head_len="3" value="查询内容"/>
						<Group name="Rec">
							<PackItem name="TmpNam">
								<Item name="GameId"   length="2"    desc="游戏编号"/>
								<Item name="DrawId"   length="5"    desc="期号"/>
								<Item name="DrawNm"   length="15"   desc="期名"/>
								<Item name="SalStr"   length="14"   desc="销售开始日期"/>
								<Item name="SalEnd"   length="14"   desc="销售结束日期"/>
								<Item name="CshStr"   length="14"   desc="兑奖开始时间"/>
								<Item name="CshEnd"   length="14"   desc="兑奖结束时间"/>
								<Item name="KenoId"   length="5"    desc="keno期号"/>
								<Item name="KenoNm"   length="15"   desc="keno期名"/>
								<Item name="KSalSt"   length="14"   desc="Keno销售开始时间"/>
								<Item name="KSalEd"   length="14"   desc="Keno销售结束时间"/>
								<Item name="ChkFlg"   length="1"    desc="对账清算标志"/>
								<Item name="ChkTim"   length="14"   desc="对帐清算时间"/>
								<Item name="PrzAmt"   length="15"   desc="返奖总金额"  expression="AMTSIMPLEFMT(DELSPACE($PrzAmt,all))"/>
								<Item name="TotAmt"   length="15"   desc="购彩总金额"  expression="AMTSIMPLEFMT(DELSPACE($TotAmt,all))"/>
								<Item name="DifFlg"   length="1"    desc="扎差标志"/>
								<Item name="DifAmt"   length="15"   desc="扎差金额"  expression="AMTSIMPLEFMT(DELSPACE($DifAmt,all))"/>
								<Item name="PayFlg"   length="1"    desc="返奖垫付标志"/>
								<Item name="PayAmt"   length="15"   desc="返奖垫付金额"  expression="AMTSIMPLEFMT(DELSPACE($PayAmt,all))"/>
								<Item name="XfeFlg"   length="1"    desc="划拨标志"/>
								<Item name="FlwCtl"   length="2"    desc="返奖流程控制标志"/>
								<Item name="ClrTim"   length="14"   desc="清算时间"/>
								<Item name="RtnTim"   length="14"   desc="返还垫付时间"/>
							</PackItem>
							<Item name="TmpNam" head_len="3"/>
						</Group>
					</PackItem>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="E">
					<Include file="etc/ERRFMT_IIT.XML"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Response>
