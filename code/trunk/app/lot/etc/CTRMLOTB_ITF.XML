				<Case value="N">
					<Item name="TxnLog"      length="15"  expression="TRIM($TxnLog,both)" desc="投注交易流水号"/>
					<Item name="SchId"       length="32"  expression="TRIM($SchId,both)"  desc="方案编号"/>
					<Item name="TLogNo"      length="15"  expression="TRIM($SchId,both)"  desc="彩票流水号"/>
					<Item name="Cipher"      length="30"  expression="TRIM($Cipher,both)" desc="彩票密码"/>
					<Item name="Verify"      length="30"  expression="TRIM($Verify,both)" desc="彩票校验码"/>
					<Item name="SchTyp"      length="1"   expression="TRIM($SchTyp,both)" desc="方案类型（1：直接投注）"/>
					<Item name="SchTit"      length="1"   expression="TRIM($SchTit,both)" desc="方案标题"/>
					<Item name="SecLev"      length="1"   expression="TRIM($SecLev,both)" desc="方案等级（1：保密投注）"/>
					<Item name="LotTxnTim"   length="14"  expression="TRIM($LotTxnTim,both)" desc="生成时间"/>
					<Item name="CityId"      length="4"   expression="TRIM($CityId,both)" desc="地市编码"/>
					<Item name="GameId"      length="2"   expression="TRIM($GameId,both)" desc="游戏编号"/>
					<Item name="DrawId"      length="5"   expression="TRIM($DrawId,both)" desc="当前大期"/>
					<Item name="KenoId"      length="5"   expression="TRIM($KenoId,both)" desc="当前小期"/>
					<Item name="BetMod"      length="5"   expression="TRIM($BetMod,both)" desc="投注方式(1：单式   12：复式   13：胆拖)"/>
					<Item name="BetMul"      length="3"   expression="TRIM($BetMul,both)" desc="投注倍数"/>
					<Item name="BetAmt"      length="15"  expression="TRIM($TxnAmt,both)" desc="投注金额"/>
					<Item name="GrpNum"      length="8"   expression="TRIM($GrpNum,both)" desc="号码组个数"/>
					<Item name="BetNum"      length="8"   expression="TRIM($BetNum,both)" desc="总号码个数"/>
					<Group name="Rec">
						<PackItem name="TmpNam">
							<Item name="BetLin"   length="128" desc="投注号码"/>
						</PackItem>
						<Item name="TmpNam" head_len="3"/>
					</Group>
					<Item name="schsts"     length="2"   expression="TRIM($schsts,both)"  desc="方案状态 1:方案提交 3:交易撤单 10:交易完成 20:未中奖 21:中小奖 22:中大奖 "/>
					<Item name="BonAmt"     length="15"  expression="TRIM($BonAmt,both)"  desc="中奖金额"/>
					<Break/>
				</Case>
				<Case value="E">
					<Include file="etc/ERRFMT_IIT.XML"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Response>
	</Transaction>
	
