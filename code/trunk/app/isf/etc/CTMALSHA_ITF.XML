  <Transaction code="482111" service="482101" desc="联通查询欠费" log_switch="1">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
      <Item name="RsFld1" length="4"/>      <!--第三方交易码-->
      <Item name="RsFld2" length="4"/>      <!--第三方业务类型-->
      <Item name="TCusID"  length="30"/>    <!--缴费号码-->
      <Item name="Feeflg"  length="1"/>     <!--充值缴费标志0欠费,1充值-->
      <Item name="Feefld"  length="2"/>     <!--联通业务类型-->
      <AddItem name="ActNo" value=" " length="21"/>     <!--电信查询时要送帐号-->
      <AddItem name="TxnAmt"  value="000000000000000"/>   <!--电信查询时要送0金额-->
      <AddItem name="AppNm"  value="GZ_TELE"/>   <!--单位类型-->
      <SwitchCode name="AppNm" new_name="CAgtNo"  tab_name="GZ_AppNm"  col_name="AppNm"/>
    </Request>
    <Response necessary="no">
      <Include file="etc/TRMTOA_IIT.XML"/>
      <Switch expression="$MsgTyp">
        <Case value="N">
          <PackItem name="TmpDat">
            <Item name="ApFmt"  length="05" value="48D06"/> <!--前端空格式码-->
            <Item name="TCusId" length="30"/>
            <Item name="TCusNm" length="60"/>
            <If condition="IS_EQUAL_STRING($Feeflg,1)">
               <Item name="OldAmt" length="15"/>
            </If>
            <Else>
               <Item name="PayAmt" length="15"/>
            </Else>
            <Item name="Arefld" length="2"/>
            <Item name="BusId"  length="20"/>
            <Item name="RsFld2" length="4"/>
          </PackItem>
          <Item name="TmpDat" head_len="4" include_len="yes"/>
          <Break/>
        </Case>
        <Case value="E">
          <PackItem name="TmpDat">
            <Item name="ApCode1" length="2" value="SC"/>
            <Item name="OFmtCd" length="3" value="D04"/>
            <Item name="RspCod" length="6"/>
            <Item name="InPos"  value="0001"/>
            <Item name="RspMsg" length="56"/>
          </PackItem>
          <Item name="TmpDat" head_len="4" include_len="yes"/>
          <Break/>
        </Case>
        <Default>
          <Item name="BillText" deli_asc="-1"/>
        </Default>
      </Switch>
    </Response>
  </Transaction>
 
