             <Item name="RspMsg" length="56"/>
             </PackItem>
             <Item name="TmpDat" head_len="4" include_len="yes"/>
             <Break/>
            </Case>
           <Case value="A">
           <Item name="BillText" deli_asc="-1" />
           <Break/>
        </Case>
      </Switch>
    </Response>
  </Transaction>
 
 
  <Transaction code="482160" service="482160" desc="电话银行外卖保险缴费交易"  log_switch="1">
