	<Transaction code="482115" service="482115" desc="车船税查询" log_switch="1">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
      <Item name="RsFld1" length="4"/>      <!--第三方交易码-->
      <Item name="RsFld2" length="4"/>      <!--第三方业务类型-->
