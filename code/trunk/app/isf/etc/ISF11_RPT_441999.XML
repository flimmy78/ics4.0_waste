       <Para label="03"  alias="SUBSTR($TxnDat,5,2)" spread="right"></Para>
       <Para label="04"  alias="SUBSTR($TxnDat,7,2)" spread="right"></Para>
       <Para label="05"  alias="$PAGESUM" spread="mid"></Para>
       <Para label="06"  alias="$PAGENUM" spread="mid"></Para>
