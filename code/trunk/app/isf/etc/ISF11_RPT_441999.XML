      </Format>
    </TableHeader>
    <Iterative line="1">
       <Para label="01" alias="SUBSTR($FTXNTM,1,8)" spread="right"></Para>
       <Para label="02" alias="羊城通自动充值" spread="right"></Para>
       <Para label="03" alias="$ActNo"  spread="right"></Para>
       <Para label="04" alias="$RSFLD2" convert="YES"  spread="right"></Para>
       <Para label="05" alias="$TCUSID"  spread="right"></Para>
       <Para label="06" alias="DELBOTHSPACE($LogNo)"  spread="right"></Para>
