<?xml version='1.0' encoding='ISO-8859-1'?>
<Application trclog="LSHA_RPT_441999.trc">
  <VarDef>
    <String alias="CAgtNo"  pos="1"/>
    <String alias="TxnDat"  pos="2"/>
    <String alias="PNodNo"  pos="3"/>
    <String alias="PTlrId"  pos="4"/>
  </VarDef>
  <MsgConvert>
    <Item alias="RSFLD2">
      <Convert value="Z01051" msg="划款交易"/>
      <Convert value="Z02011" msg="退款交易"/>
      <DEFAULT msg="无此交易"/>
    </Item>
    <Item alias="HTxnSt">
      <Convert value="S" msg="交易成功"/>
      <DEFAULT msg="可疑,请查询"/>
    </Item>
  </MsgConvert>
 
  <DataSource object="sql">
    <Iterative>
       <Sql sql="select ActNo,TCUSID,TxnAmt,HTxnSt,TckNo,LogNo,FTXNTM,RSFLD2 from afetxnjnl where ActDat ='%s' and CAgtNo in('%s','4410001102') and HTxnSt='S' and TTxnst IN ('S','T') order by RSFLD2">
       <FieldList>$TxnDat|$CAgtNo|</FieldList>
	   </Sql>
    </Iterative>
    <SubTotal type="field" groupby="RSFLD2|">
			<Field fmtseq="1"  field="RSFLD2">
				<Sum sum="TXNAMT" sum_alias="SubAmt" count="CntAmt"/>
			</Field>
		</SubTotal>
		<Summed type="field">
			<Field sum="TXNAMT" sum_alias="sumamt" count="TotCnt"/>
		</Summed>   

  </DataSource>
  <FormatDefine pagerows="41" pagesum="YES">
