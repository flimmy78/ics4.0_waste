<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- 交通银行广东省分行商户POS失败清单 -->
<Application dbconnect="YES" trclog="Pcl013.log">
	<VarDef>
		<String alias="ClrDat" pos="1"/>
		<String alias="ActDat" pos="2"/>
		<String alias="PrtTlr" pos="3"/>
		<String alias="OpnBr"  pos="4"/>
	</VarDef>
	<DataSource object="sql">
		<Iterative>
			<Sql sql="select RefNo, CtrDt, CtrTime, TranAcc, TranAmt, TxnAmt, FeeAmt, MerId, TermId, ActNo, ActNm, HRSPCD, ERRMSG
									from PclDTxnDtl
								 where CtrPostDate='%s' and TxnSts!='S'
							order by MerId, TermId, CtrTime">
				<FieldList>$ClrDat|</FieldList>
			</Sql>
		</Iterative>
		<SubTotal type="field" groupby="MERID|">
			<Field fmtseq="1"  field="MERID">
				<Sum sum="TRANAMT" sum_alias="SubTranAmt" count="SubNum"/>
				<Sum sum="TXNAMT"  sum_alias="SubTxnAmt"/>
				<Sum sum="FEEAMT"  sum_alias="SubFeeAmt"/>
			</Field>
		</SubTotal>
		<Summed type="field">
			<Field sum="TRANAMT" sum_alias="TotTranAmt" count="TotNum"/>
			<Field sum="TXNAMT"  sum_alias="TotTxnAmt"/>
			<Field sum="FEEAMT"  sum_alias="TotFeeAmt"/>
		</Summed>   
	</DataSource>

	<FormatDefine pagerows="45" pagesum="YES">
		<TableHeader>
			<Para label="01" alias="$ActDat"/>
			<Para label="02" alias="$ClrDat"/>
			<Para label="03" alias="$OpnBr"/>
			<Para label="04" alias="$PAGESUM"/>
			<Para label="05" alias="$PAGENUM"/>
			<Format>
                                                     交通银行转帐POS失败清单
                                       ==========================================================
  入帐日期：#                                                                                                                            银联清算日期：#
  分行号：#                                                                                                                              共 # 页     第 # 页
┏━━━━┳━━━━┳━━━━━━┳━━━━━━━━━━━┳━━━━━━━━┳━━━━━━━━┳━━━━━━━━┳━━━━━━━━━━━┳━━━━┳━━━━━┓
┃交易日期┃交易时间┃ 系统参考号 ┃       转出帐号       ┃    交易金额    ┃    清算金额    ┃   手续费金额   ┃       入账卡号       ┃入账户名┃主机返回码┃
┣━━━━╋━━━━╋━━━━━━╋━━━━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━━━━╋━━━━╋━━━━━┫
			</Format>
		</TableHeader>
		<Iterative line="1">
			<Para label="01" alias="$CTRDT"           spread="mid" />
			<Para label="02" alias="$CTRTIME"         spread="mid" />
			<Para label="03" alias="$REFNO"           spread="mid" />
			<Para label="04" alias="$TRANACC"         spread="mid" />
			<Para label="05" alias="AMTFMT($TRANAMT)" spread="left"/>
			<Para label="06" alias="AMTFMT($TXNAMT)"  spread="left"/>
			<Para label="07" alias="AMTFMT($FEEAMT)"  spread="left"/>
			<Para label="08" alias="$ACTNO"           spread="mid" />
			<Para label="09" alias="$ACTNM"           spread="mid" />
			<Para label="10" alias="$ERRMSG"          spread="mid" />
			<Format>
┃#       ┃#       ┃#           ┃#                     ┃               #┃               #┃               #┃#                     ┃#       ┃#         ┃
			</Format>
		</Iterative>
		<SubTotal fmtseq="1">
			<Para label="01" alias="$MERID"              spread="right"/>
			<Para label="02" alias="$TERMID"             spread="right"/>
			<Para label="03" alias="AMTFMT($SubTranAmt)" spread="left"/>
			<Para label="04" alias="AMTFMT($SubTxnAmt)"  spread="left"/>
			<Para label="05" alias="AMTFMT($SubFeeAmt)"  spread="left"/>
			<Para label="06" alias="$SubNum"             spread="right" />
			<Format>
┣━━━━╋━━━━┻━━━━━━╋━━━━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━━━━┻━━━━┻━━━━━┫
┃小计：  ┃商户号：#             ┃终端号：#             ┃               #┃               #┃               #┃异常笔数：#                                 ┃
┣━━━━╋━━━━┳━━━━━━╋━━━━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━━━━┳━━━━┳━━━━━┫
			</Format>
		</SubTotal>
		<Summed>
			<Para label="01" alias="AMTFMT($TotTranAmt)" spread="left"/>
			<Para label="02" alias="AMTFMT($TotTxnAmt)"  spread="left"/>
			<Para label="03" alias="AMTFMT($TotFeeAmt)"  spread="left"/>
			<Para label="04" alias="$TotNum"					   spread="right"/>
			<Format>
┣━━━━╋━━━━┻━━━━━━┻━━━━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━━━━┻━━━━┻━━━━━┫
┃合计：  ┃                                              ┃               #┃               #┃               #┃异常笔数：#                                 ┃
┣━━━━╋━━━━┳━━━━━━┳━━━━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━╋━━━━━━━━━━━┳━━━━┳━━━━━┫
			</Format>
		</Summed>
		<TableFooter>
			<Para label="01" alias="$PrtTlr"/>
			<Para label="02" alias="$ActDat"/>
			<Format>
┗━━━━┻━━━━┻━━━━━━┻━━━━━━━━━━━┻━━━━━━━━┻━━━━━━━━┻━━━━━━━━┻━━━━━━━━━━━┻━━━━┻━━━━━┛
                                打印柜员号：#                                        打印日期：#
			</Format>
		</TableFooter>
	</FormatDefine>
</Application>
