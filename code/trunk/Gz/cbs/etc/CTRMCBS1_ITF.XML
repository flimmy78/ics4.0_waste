          <Switch expression="$MsgTyp">
            <Case value="N">
              <PackItem name="TmpDat">
                <Item name="ApCode" length="2" value="46"/>
                <Item name="OFmtCd" length="3" value="999"/>
                <Item name="OBankNo" length="12" desc="行号"/>
              </PackItem>
              <Item name="TmpDat" head_len="4" include_len="yes"/>
