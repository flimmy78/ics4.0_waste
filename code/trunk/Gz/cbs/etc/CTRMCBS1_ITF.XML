                <Item name="OFmtCd"   length=" 3" value="999"/>
                <Item name="OAccIn01" length="21" desc="内部帐号"/>
                <Item name="OAccIn02" length="21" desc="内部帐号"/>
                <Item name="OAccIn03" length="21" desc="内部帐号"/>
                <Item name="OAccSq01" length=" 5" desc="分户序号"/>
                <Item name="OAccSq02" length=" 5" desc="分户序号"/>
                <Item name="OAccSq03" length=" 5" desc="分户序号"/>
                <Item name="OAccSq04" length=" 5" desc="分户序号"/>
                <Item name="OAccSq05" length=" 5" desc="分户序号"/>
                <Item name="OAccSq06" length=" 5" desc="分户序号"/>
                <Item name="OAccSq07" length=" 5" desc="分户序号"/>
                <Item name="OAccSq08" length=" 5" desc="分户序号"/>
                <Item name="OAccSq09" length=" 5" desc="分户序号"/>
                <Item name="OAccSq10" length=" 5" desc="分户序号"/>
