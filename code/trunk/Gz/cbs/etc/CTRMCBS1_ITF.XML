          <Item name="AccSq01" length=" 5" desc="分户序号"/>
          <Item name="AccSq02" length=" 5" desc="分户序号"/>
          <Item name="AccSq03" length=" 5" desc="分户序号"/>
          <Item name="AccSq04" length=" 5" desc="分户序号"/>
          <Item name="AccSq05" length=" 5" desc="分户序号"/>
          <Item name="AccSq06" length=" 5" desc="分户序号"/>
