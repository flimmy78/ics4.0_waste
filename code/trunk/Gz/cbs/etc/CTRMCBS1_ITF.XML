                <Item name="VchNo"  length=" 8" desc="凭证号码"/>
                <SwitchCode name="CcyCod" tab_name="CcyCod" col_name="CcyCod"/>
                <Item name="CcyCod" length=" 6" desc="币种"/>
                <Item name="TxnAmt" length="15" desc="交易金额"/>
                <SwitchCode name="BilSts" tab_name="BilSts" col_name="BilSts"/>
                <Item name="BilSts" length="10" desc="明细状态"/>
                <Item name="ClrDat" length=" 8" desc="清算日期"/>
                <Item name="RtnCod" length=" 8" desc="返回码"/>
                <Item name="ActDat" length=" 8" desc="会计日期"/>
                <Item name="NodNo"  length=" 6" desc="交易网点"/>
                <Item name="TckNo"  length="11" desc="流水号"/>
