  <Transaction code="465721" service="465721" timeout="30" log_switch="0" desc="人行信息代码浏览">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
      <If condition="IS_EQUAL_STRING($TIATyp,P)">
        <Item name="PagIdx" length="6"/>
      </If>
      <Else>
        <Item name="CodTyp" length=" 6" expression="DELSPACE($CodTyp,all)" desc="代码种类"/>
        <Item name="Code"   length="18" expression="DELSPACE($Code,all)"   desc="代码"/>
        <Item name="CodSmr" length="40" expression="DELSPACE($CodSmr,all)" desc="中文"/>
      </Else>
    </Request>
    <Response necessary="no">
      <Include file="etc/TRMTOA_IIT.XML"/>
      <If condition="IS_EQUAL_STRING($MsgTyp,N)">
        <PackItem name="TmpDat">
          <Item name="ApCode"  length="2" value="32"/>
          <Item name="OFmtCd"  length="3" value="Z01"/>
          <Item name="PageNo"  length="4" value="0001"/>
          <Item name="VarSize" length="1" value="3"/>
          <Item name="Ttl"     head_len="3" value="人行信息代码浏览"/>
          <Item name="SubTtl"  head_len="3" value="查询内容"/>
          <Group name="Rec" repeat_name="RecNum">
            <PackItem name="TmpNam">
              <Item name="CodTyp" length=" 6" desc="代码种类"/>
              <Item name="Code"   length="18" desc="代码"/>
              <Item name="CodSmr" length="40" desc="代码中文"/>
            </PackItem>
            <Item name="TmpNam"   head_len="3"/>
          </Group>
        </PackItem>
        <Item name="TmpDat" head_len="4" include_len="yes"/>
      </If>
      <Else>
        <Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
      </Else>
    </Response>
  </Transaction>


  <Transaction code="465722" service="465722" timeout="30" log_switch="0" desc="汇兑费率资料浏览">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
      <If condition="IS_EQUAL_STRING($TIATyp,P)">
        <Item name="PagIdx" length="6"/>
      </If>
      <Else>
        <Item name="TxnKnd" length="6" desc="交易类型代码"/>
        <Item name="UsgCod" length="3" desc="资金用途代码"/>
        <Item name="LclFlg" length="1" desc="同城异地标志"/>
        <Item name="CcyCod" length="3" desc="币种"/>
      </Else>
    </Request>
    <Response necessary="no">
      <Include file="etc/TRMTOA_IIT.XML"/>
      <If condition="IS_EQUAL_STRING($MsgTyp,N)">
        <PackItem name="TmpDat">
          <Item name="ApCode"  length="2" value="32"/>
          <Item name="OFmtCd"  length="3" value="Z01"/>
          <Item name="PageNo"  length="4" value="0001"/>
          <Item name="VarSize" length="1" value="3"/>
          <Item name="Ttl"     head_len="3" value="汇兑费率资料浏览"/>
          <Item name="SubTtl"  head_len="3" value="查询内容"/>
          <Group name="Rec" repeat_name="RecNum">
            <PackItem name="TmpNam">
              <Item name="Sts"    length=" 1" desc="状态"/>
              <Item name="TxnKnd" length=" 6" desc="交易类型代码"/>
              <Item name="UsgCod" length=" 3" desc="资金用途代码"/>
              <Item name="LclFlg" length=" 1" desc="同城异地标志"/>
              <Item name="CcyCod" length=" 3" desc="币种"/>
              <Item name="MinAmt" length="15" desc="最低金额"/>
              <Item name="MaxAmt" length="15" desc="最高金额"/>
              <Item name="FeeMod" length=" 1" desc="手续费收费模式"/>
              <Item name="FeeRat" length=" 6" expression="FDIV($FeeRat,10000,5)" desc="手续费比例"/>
              <Item name="FeeAmt" length="15" desc="手续费额度"/>
              <Item name="PstMod" length=" 1" desc="邮电费收费模式"/>
              <Item name="PstRat" length=" 6" expression="FDIV($PstRat,10000,5)" desc="邮电费比例"/>
              <Item name="PstAmt" length="15" desc="邮电费额度"/>
              <Item name="ValDt"  length=" 8" desc="生效日期"/>
            </PackItem>
            <Item name="TmpNam"   head_len="3"/>
          </Group>
        </PackItem>
        <Item name="TmpDat" head_len="4" include_len="yes"/>
      </If>
      <Else>
        <Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
      </Else>
    </Response>
  </Transaction>


  <Transaction code="465723" service="465723" timeout="30" log_switch="0" desc="行名行号浏览">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
      <If condition="IS_EQUAL_STRING($TIATyp,P)">
        <Item name="PagIdx" length="6"/>
      </If>
      <Else>
        <Item name="BankNo" length="12" expression="DELSPACE($BankNo,all)" desc="行号"/>
        <Item name="TxnKnd" length=" 6" desc="交易种类"/>
        <Item name="BankNm" length="60" expression="DELSPACE($BankNm,all)" desc="行名"/>
      </Else>
    </Request>
    <Response necessary="no">
      <Include file="etc/TRMTOA_IIT.XML"/>
      <Switch expression="$MsgTyp">
        <Case value="N">
          <PackItem name="TmpDat">
            <Item name="ApCode"  length="2" value="32"/>
            <Item name="OFmtCd"  length="3" value="Z01"/>
            <Item name="PageNo"  length="4" value="0001"/>
            <Item name="VarSize" length="1" value="3"/>
            <Item name="Ttl"     head_len="3" value="人行信息代码浏览"/>
            <Item name="SubTtl"  head_len="3" value="查询内容"/>
            <Group name="Rec" repeat_name="RecNum">
              <PackItem name="TmpNam">
                <Item name="BankNo" length="12" desc="行号"/>
                <Item name="BankNm" length="50" desc="行名"/>
                <Item name="EffDat" length=" 8" desc="生效日期"/>
                <Item name="IvdDat" length=" 8" desc="失效日期"/>
                <Item name="TelNo"  length="20" desc="联系电话"/>
              </PackItem>
              <Item name="TmpNam"   head_len="3"/>
            </Group>
          </PackItem>
          <Item name="TmpDat" head_len="4" include_len="yes"/>
          <Break/>
        </Case>
        <Case value="E">
          <PackItem name="TmpDat">
            <Item name="ApCode" length="2" value="SC"/>
            <Item name="OFmtCd" length="3" value="D04"/>
            <Item name="RspCod" length="6"/>
            <Item name="InPos"  length="4" value="0001"/>
