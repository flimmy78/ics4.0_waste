          </Switch>
          <Break/>
        </Case>
        <Default>
          <Quote name="CBS_RES_1"/>
          <Break/>
        </Default>
      </Switch>
    </Response>
  </Transaction>


  <Transaction code="465703" service="465703" timeout="30" log_switch="0" desc="交换行号对照表维护">
