<?xml version='1.0' encoding='ISO-8859-1'?>
<Application name="CBS" server="OFRTCBS1" pre_dll="dll/pre_term.so" pre_func="pre_term" spe_dll="dll/pre_term.so" spe_func="spe_term" timeout="60">
  <CodeSwitching>
    <Table name="TxnKnd" file="etc/CBS_CSW.XML">
      <Column name="TxnKnd"    sort="no"/>
      <Column name="TxnKndSmr" sort="no"/>
    </Table>
    <Table name="OIFlag" file="etc/CBS_CSW.XML">
      <Column name="OIFlag"    sort="no"/>
      <Column name="OIFlagSmr" sort="no"/>
    </Table>
    <Table name="ActFlg" file="etc/CBS_CSW.XML">
      <Column name="ActFlg"    sort="no"/>
      <Column name="ActFlgSmr" sort="no"/>
    </Table>
    <Table name="AVchTp" file="etc/CBS_CSW.XML">
      <Column name="AVchTp"    sort="no"/>
      <Column name="AVchTpSmr" sort="no"/>
    </Table>
    <Table name="VchTyp" file="etc/CBS_CSW.XML">
      <Column name="VchTyp"    sort="no"/>
      <Column name="VchTypSmr" sort="no"/>
    </Table>
    <Table name="CcyCod" file="etc/CBS_CSW.XML">
      <Column name="CcyCod"    sort="no"/>
      <Column name="CcyCodSmr" sort="no"/>
    </Table>
    <Table name="BilSts" file="etc/CBS_CSW.XML">
      <Column name="BilSts"    sort="no"/>
      <Column name="BilStsSmr" sort="no"/>
    </Table>
    <Table name="PstTyp" file="etc/CBS_CSW.XML">
      <Column name="PstTyp"    sort="no"/>
      <Column name="PstTypSmr" sort="no"/>
    </Table>
    <Table name="SmrCod" file="etc/CBS_CSW.XML">
      <Column name="SmrCod"    sort="no"/>
      <Column name="SmrCodSmr" sort="no"/>
    </Table>
    <Table name="UsgCod" file="etc/CBS_CSW.XML">
      <Column name="UsgCod"    sort="no"/>
      <Column name="UsgCodSmr" sort="no"/>
    </Table>
    <Table name="TBusTp" file="etc/CBS_CSW.XML">
      <Column name="TBusTp"    sort="no"/>
      <Column name="TBusTpSmr" sort="no"/>
    </Table>
    <Table name="FeeFlg" file="etc/CBS_CSW.XML">
      <Column name="FeeFlg"    sort="no"/>
      <Column name="FeeFlgSmr" sort="no"/>
    </Table>
    <Table name="EtsFlg" file="etc/CBS_CSW.XML">
      <Column name="EtsFlg"    sort="no"/>
      <Column name="EtsFlgSmr" sort="no"/>
    </Table>
    <Table name="BchFlgToTblNam" file="etc/CBS_CSW.XML">
      <Column name="BchFlg" sort="no"/>
      <Column name="TblNam" sort="no"/>
    </Table>
		<Table name="DtlSts" file="etc/CBS_CSW.XML">
			<Column name="DtlSts"    sort="no"/>
			<Column name="DtlStsSmr" sort="no"/>
		</Table>
  </CodeSwitching>

  <Define>
<!--通用回应-->
    <Macro name="CBS_RES_1">
      <Switch expression="$MsgTyp">
        <Case value="N">
          <CopyItem dest_name="RspCod" sour_name="HRspCd" />
          <Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
          <Break/>
        </Case>
        <Case value="W"/>
        <Case value="E">
          <PackItem name="TMP">
            <Item name="ApCode" length="2" value="SC"/> 
            <Item name="OFmtCd" length="3" value="D04"/>
            <Item name="RspCod" length="6"/>
            <Item name="InPos"  length="4" value="0001"/>
            <Item name="RspMsg" length="56" />
          </PackItem>
          <Item name="TMP" head_len="4" include_len="yes"/>
          <Break/>
        </Case>
        <Case value="A">
          <Item name="BillText" deli_asc="-1"/>
          <Break/>
        </Case>
      </Switch>
    </Macro>
  </Define>


  <Transaction code="465701" service="465701" timeout="30" log_switch="0" desc="汇兑费率资料维护">
    <Request>
      <Include file="etc/TRMTIA_IIT.XML"/>
      <Item name="Func"   length=" 1" desc="功能选择"/>
      <Item name="TxnKnd" length=" 6" desc="交易类型代码"/>
      <Item name="TxnNam" length="60" expression="DELSPACE($TxnNam,all)" desc="业务类型中文"/>
      <Item name="UsgCod" length=" 3" desc="资金用途代码"/>
      <Item name="UsgNam" length="60" expression="DELSPACE($UsgNam,all)" desc="资金用途中文"/>
      <Item name="LclFlg" length=" 1" desc="同城异地标志"/>
      <Item name="CcyCod" length=" 3" desc="币种"/>
      <Item name="MinAmt" length="15" desc="最低金额"/>
      <Item name="MaxAmt" length="15" desc="最高金额"/>
      <Switch expression="$Func">
        <Case value="0"/>
        <Case value="1"/>
        <Case value="3">
          <Item name="FeeMod" length=" 1" desc="手续费收费模式"/>
          <Item name="PstMod" length=" 1" desc="邮电费收费模式"/>
          <Item name="FeeRat" length=" 5" desc="手续费比例"/>
          <Item name="PstRat" length=" 5" desc="邮电费比例"/>
          <Item name="FeeAmt" length="15" desc="手续费额度"/>
          <Item name="PstAmt" length="15" desc="邮电费额度"/>
          <Item name="ValDt"  length=" 8" desc="生效日期"/>
          <Item name="Sts"    length=" 1" desc="状态"/>
          <Break/>
        </Case>
        <Default>
          <Break/>
        </Default>
      </Switch>
    </Request>
    <Response necessary="no">
      <Include file="etc/TRMTOA_IIT.XML"/>
      <Switch expression="$Func">
        <Case value="2">
          <Switch expression="$MsgTyp">
            <Case value="N">
              <PackItem name="TmpDat">
                <Item name="ApCode"  length="2" value="46"/>
                <Item name="OFmtCd"  length="3" value="999"/>
                <Item name="OFeeMod" length=" 1" desc="手续费收费模式"/>
                <Item name="OPstMod" length=" 1" desc="邮电费收费模式"/>
                <Item name="OFeeRat" length=" 5" desc="手续费比例"/>
                <Item name="OPstRat" length=" 5" desc="邮电费比例"/>
                <Item name="OFeeAmt" length="15" desc="手续费额度"/>
                <Item name="OPstAmt" length="15" desc="邮电费额度"/>
                <Item name="OValDt"  length=" 8" desc="生效日期"/>
                <Item name="OSts"    length=" 1" desc="状态"/>
              </PackItem>
              <Item name="TmpDat" head_len="4" include_len="yes"/>
              <Break/>
            </Case>
            <Case value="E">
              <PackItem name="TmpDat">
                <Item name="ApCode" length="2" value="SC"/>
                <Item name="OFmtCd" length="3" value="D04"/>
                <Item name="RspCod" length="6"/>
                <Item name="InPos"  length="4" value="0001"/>
                <Item name="RspMsg" length="56"/>
              </PackItem>
              <Item name="TmpDat" head_len="4" include_len="yes"/>
              <Break/>
            </Case>
            <Default>
              <Item name="BillText" deli_asc="-1"/>
              <Break/>
            </Default>
          </Switch>
          <Break/>
        </Case>
        <Default>
          <Quote name="CBS_RES_1"/>
          <Break/>
        </Default>
      </Switch>
    </Response>
  </Transaction>


  <Transaction code="465702" service="465702" timeout="30" log_switch="0" desc="系统运行参数维护">
