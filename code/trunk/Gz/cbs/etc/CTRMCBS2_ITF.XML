					<Case value="1">
						<Item name="TlrNo" length=" 7" expression="DELSPACE($TlrNo,all)" desc="柜员号"/>
						<Item name="TckNo" length="11" expression="DELSPACE($TckNo,all)" desc="流水号"/>
