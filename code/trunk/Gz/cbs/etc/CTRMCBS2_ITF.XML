<?xml version='1.0' encoding='ISO-8859-1'?>
<Application name="CBS" server="OFRTCBS2" pre_dll="dll/pre_term.so" pre_func="pre_term" spe_dll="dll/pre_term.so" spe_func="spe_term" timeout="300">
<!--代码转换定义-->
	<CodeSwitching>
		<Table name="BchFlgToTblNam" file="etc/CBS_CSW.XML">
			<Column name="BchFlg" sort="no"/>
			<Column name="TblNam" sort="no"/>
		</Table>
	</CodeSwitching>

<!--接口定义-->
	<Define>
		<Macro name="CBS_RES_2">
			<Switch expression="$MsgTyp">
				<Case value="N">
					<CopyItem dest_name="RspCod" sour_name="HRspCd"/>
					<CopyItem dest_name="BillText" sour_name="Back"/>
					<Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
					<Break/>
				</Case>
				<Case value="E">
					<PackItem name="TMP">
						<Item name="ApCode" length=" 2" value="SC"/> 
						<Item name="OFmtCd" length=" 3" value="D04"/>
						<Item name="RspCod" length=" 6"/>
						<Item name="InPos"  length=" 4" value="0001"/>
						<Item name="RspMsg" length="56" />
					</PackItem>
					<Item name="TMP" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Macro>

<!--查复业务录入-->
		<Macro name="465812_REQ">
			<If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx"  length="6"/>
				<AddItem name="Func" value="1"/>
			</If>
			<Else>
				<Item name="Func"   length="1"  /> <!--功能选择-->
				<Switch expression="$Func">
					<Case value="1">	<!--查询-->
						<Break/>
					</Case>
					<Case value="2"/>	<!--单笔查询-->
					<Case value="3">	<!--处理-->
						<Item name="TxnKnd" length=" 6" /> <!--交易类型-->
						<Item name="SndDat" length=" 8" /> <!--发起行日期-->
						<Item name="SndBNo" length="12" /> <!--发起行行号-->
						<Item name="MsgSeq" length="16" /> <!--信息序号-->
						<Item name="OIFlag" length=" 1" /> <!--往来标志-->
						<If condition="IS_EQUAL_STRING($Func,3)">
							<Item name="RspSmr" length="300" expression="DELSPACE($RspSmr,all)" /> <!--查复内容-->
						</If>
						<Break/>
					</Case>
				</Switch>
			</Else>
		</Macro>
		<Macro name="465812_RES">
			<Switch expression="$MsgTyp">
				<Case value="N">
					<Switch expression="$Func">
						<Case value="1">
							<PackItem name="TmpDat">
								<Item name="ApCode"  length="2" value="32"/>
								<Item name="OFmtCd"  length="3" value="Z01"/>
								<Item name="PageNo"  length="4" value="0001"/>
								<Item name="VarSize" length="1" value="3"/>
								<Item name="Ttl"     head_len="3" value="网点资料浏览"/>
								<Item name="SubTtl"  head_len="3" value="查询内容"/>
								<Group name="Rec" repeat_name="RecNum">
									<PackItem name="TmpNam">
										<Item name="QryTyp"  length=" 3" /> <!--查询种类-->
										<Item name="SOpnBk"  length="12" /> <!--发起人开户行行号-->
										<Item name="PTraKnd" length=" 4" /> <!--原交易类型-->
										<Item name="PSndBNo" length="12" /> <!--原交易发起行行号-->
										<Item name="PRcvBNo" length="12" /> <!--原交易接收行行号-->
										<Item name="PSndDat" length=" 8" /> <!--原交易委托日期-->
										<Item name="PCcyCod" length=" 3" /> <!--原交易币种-->
										<Item name="PTxnAmt" length="15" /> <!--原交易金额-->
										<Item name="PVchId"  length=" 8" /> <!--原交易凭证提交号-->
										<Item name="PPayAct" length="35" /> <!--原付款人帐号-->
										<Item name="PPayNam" length="60" /> <!--原付款人户名-->
										<Item name="PGatAct" length="35" /> <!--原接收人帐号-->
										<Item name="PGatNam" length="60" /> <!--原接收人户名-->
										<Item name="TxnKnd"  length=" 6" /> <!--原交易类型-->
										<Item name="SndDat"  length=" 8" /> <!--原发送日期-->
										<Item name="SndBNo"  length="12" /> <!--发起行行号-->
										<Item name="MsgSeq"  length="16" /> <!--信息序号-->
										<Item name="OIFlag"  length=" 1" /> <!--往来标志-->
									</PackItem>
									<Item name="TmpNam"   head_len="3"/>
								</Group>
							</PackItem>
							<Break/>
						</Case>
						<Case value="2">
							<PackItem name="TmpDat">
								<Item name="ApCode"  length=" 2" value="46"/>
								<Item name="OFmtCd"  length=" 3" value="999"/>
								<Item name="QryTyp"  length=" 3" /> <!--查询种类-->
								<Item name="SOpnBk"  length="12" /> <!--发起人开户行行号-->
								<Item name="ReqSmr"  length="300" expression="TOSBC(DELSPACE($ReqSmr,all))"/> <!--查询内容-->
								<Item name="TxnKnd"  length=" 6" /> <!--原交易类型-->
								<Item name="SndDat"  length=" 8" /> <!--原发送日期-->
								<Item name="SndBNo"  length="12" /> <!--发起行行号-->
								<Item name="MsgSeq"  length="16" /> <!--信息序号-->
								<Item name="OIFlag"  length=" 1" /> <!--往来标志-->
							</PackItem>
							<Break/>
						</Case>
						<Case value="3">
							<PackItem name="TmpDat">
								<Item name="ApCode" length="2" value="46"/>
								<Item name="OFmtCd" length="3" value="999"/>
								<Item name="RspSeq" length="8" /> <!--查复书编号-->
							</PackItem>
							<Break/>
						</Case>
					</Switch>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="W"/>
				<Case value="E">
					<PackItem name="TMP">
						<Item name="ApCode" length=" 2" value="SC"/> 
						<Item name="OFmtCd" length=" 3" value="D04"/>
						<Item name="RspCod" length=" 6"/>
						<Item name="InPos"  length=" 4" value="0001"/>
						<Item name="RspMsg" length="56"/>
					</PackItem>
					<Item name="TMP" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Macro>
<!--查询查复业务确认（删除）-->
		<Macro name="465813_REQ">
			<If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx"   length="6"/>
				<AddItem name="Func" value="1"/>
			</If>
			<Else>
				<Item name="Func"   length="1"  /> <!--功能选择-->
				<Switch expression="$Func">
					<Case value="1">	<!--查询-->
						<Item name="TlrNo"  length="7" expression="DELSPACE($TlrNo,both)" /> <!--录入柜员-->
						<Item name="QryKnd" length="1" /> <!--查询查复类型-->
						<Break/>
					</Case>
					<Case value="2"/>	<!--单笔查询-->
					<Case value="3">	<!--处理-->
						<Item name="TxnKnd" length="6"  /> <!--交易类型-->
						<Item name="SndDat" length="8"  /> <!--发起行行号-->
						<Item name="SndBNo" length="12" /> <!--发起行行号-->
						<Item name="MsgSeq" length="16" /> <!--信息序号-->
						<Item name="OIFlag" length="1"  /> <!--往来标志-->
						<If condition="IS_EQUAL_STRING($Func,3)">
							<Item name="OprCod" length="1"  /> <!--处理方式-->
						</If>
						<Break/>
					</Case>
				</Switch>
			</Else>
		</Macro>
		<Macro name="465813_RES">
			<Switch expression="$MsgTyp">
				<Case value="N">
					<Switch expression="$Func">
						<Case value="1">
							<PackItem name="TmpDat">
								<Item name="ApCode"  length="2" value="32"/>
								<Item name="OFmtCd"  length="3" value="Z01"/>
								<Item name="PageNo"  length="4" value="0001"/>
								<Item name="VarSize" length="1" value="3"/>
								<Item name="Ttl"     head_len="3" value="网点资料浏览"/>
								<Item name="SubTtl"  head_len="3" value="查询内容"/>
								<Group name="Rec" repeat_name="RecNum">
									<PackItem name="TmpNam">
										<Item name="QryTyp"  length=" 3" /> <!--查询种类-->
										<Item name="SOpnBk"  length="12" /> <!--发起人开户行行号-->
										<Item name="PTraKnd" length=" 4" /> <!--原交易类型-->
										<Item name="PSndBNo" length="12" /> <!--原交易发起行行号-->
										<Item name="PRcvBNo" length="12" /> <!--原交易接收行行号-->
										<Item name="PSndDat" length=" 8" /> <!--原交易委托日期-->
										<Item name="PCcyCod" length=" 3" /> <!--原交易币种-->
										<Item name="PTxnAmt" length="15" /> <!--原交易金额-->
										<Item name="PVchId"  length=" 8" /> <!--原交易凭证提交号-->
										<Item name="PPayAct" length="35" /> <!--原付款人帐号-->
										<Item name="PPayNam" length="60" /> <!--原付款人户名-->
										<Item name="PGatAct" length="35" /> <!--原接收人帐号-->
										<Item name="PGatNam" length="60" /> <!--原接收人户名-->
										<Item name="TxnKnd"  length=" 6" /> <!--原交易类型-->
										<Item name="SndDat"  length=" 8" /> <!--原发送日期-->
										<Item name="SndBNo"  length="12" /> <!--发起行行号-->
										<Item name="MsgSeq"  length="16" /> <!--信息序号-->
										<Item name="OIFlag"  length=" 1" /> <!--往来标志-->
									</PackItem>
									<Item name="TmpNam"   head_len="3"/>
								</Group>
							</PackItem>
							<Break/>
						</Case>
						<Case value="2">
							<PackItem name="TmpDat">
								<Item name="ApCode"  length=" 2" value="46"/>
								<Item name="OFmtCd"  length=" 3" value="999"/>
								<Item name="PTraKnd" length=" 4" /> <!--交易类型-->
								<Item name="PVchId"  length=" 8" /> <!--凭证提交号-->
								<Item name="PSndBNo" length="12" /> <!--原发起行行号-->
								<Item name="PSndDat" length=" 8" /> <!--原委托日期-->
								<Item name="PPayAct" length="35" /> <!--原发起人帐号-->
								<Item name="PPayNam" length="60" /> <!--原发起人户名-->
								<Item name="PCcyCod" length=" 3" /> <!--原币种-->
								<Item name="PTxnAmt" length="15" /> <!--原交易金额-->
								<Item name="PGatAct" length="35" /> <!--原接收人帐号-->
								<Item name="PGatNam" length="60" /> <!--原接收人户名-->
								<Item name="QryTyp"  length=" 3" /> <!--查询种类-->
								<Item name="Smr"     length="300" expression="DELSPACE($Smr,all)"/> <!--内容-->
								<Item name="TxnKnd"  length=" 6" /> <!--交易类型细分-->
								<Item name="SndDat"  length=" 8" /> <!--发起日期-->
								<Item name="SndBNo"  length="12" /> <!--发起行行号-->
								<Item name="MsgSeq"  length="16" /> <!--信息序号-->
								<Item name="OIFlag"  length=" 1" /> <!--往来标志-->
							</PackItem>
							<Break/>
						</Case>
						<Default>
							<Item name="BillText" deli_asc="-1" pro_dll="dll/pre_sp.so" pro_func="spe_asctohex"/>
							<Break/>
						</Default>
					</Switch>
					<If condition="OR(IS_EQUAL_STRING($Func,1),IS_EQUAL_STRING($Func,2))">
						<Item name="TmpDat" head_len="4" include_len="yes"/>
					</If>
					<Break/>
				</Case>
				<Case value="W"/>
				<Case value="E">
					<PackItem name="TMP">
						<Item name="ApCode" length=" 2" value="SC"/> 
						<Item name="OFmtCd" length=" 3" value="D04"/>
						<Item name="RspCod" length=" 6"/>
						<Item name="InPos"  length=" 4" value="0001"/>
						<Item name="RspMsg" length="56"/>
					</PackItem>
					<Item name="TMP" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Macro>
<!--定期借记业务协议维护-->
		<Macro name="465820_REQ">
			<Item name="Func"  length=" 1" /> <!--功能选择-->
			<Item name="AgrNo" length="60" expression="DELSPACE($AgrNo,both)" /> <!--协议-->
			<Switch expression="$Func">
				<Case value="0"/>
				<Case value="1"/>
				<Case value="3">
					<Item name="SBusTy" length=" 5" /> <!--业务类型-->
					<Item name="ActFlg" length=" 1" /> <!--账户性质-->
					<Item name="PyrAct" length="32" expression="DELSPACE($PyrAct,both)" /> <!--付款人帐号-->
					<Item name="PyrNam" length="60" expression="DELSPACE($PyrNam,both)" /> <!--付款人账户-->
					<Item name="PyrOpn" length="12" /> <!--付款人开户行-->
					<Item name="UniCod" length="20" expression="DELSPACE($UniCod,both)" /> <!--收款单位编码-->
					<Item name="PyeAct" length="32" expression="DELSPACE($PyeAct,both)" /> <!--收款人帐号-->
					<Item name="PyeNam" length="60" expression="DELSPACE($PyeNam,both)" /> <!--收款人账户-->
					<Item name="PyeOpn" length="12" /> <!--收款人开户行-->
					<Item name="Smr"    length="60" expression="DELSPACE($Smr,both)" /> <!--备注-->
					<Item name="pswd"   length="20" /> <!--密码-->
					<Item name="TCusId" length=" 8" /> <!--借用表中该字段存放存折凭证号码-->
					<Item name="CrtMod" length=" 1" /> <!--协议产生方式-->
					<Break/>
				</Case>
				<Default>
					<Break/>
				</Default>
			</Switch>
		</Macro>
		<Macro name="465820_RES">
			<Switch expression="$MsgTyp">
				<Case value="N">
					<Switch expression="$Func">
						<Case value="0">
							<PackItem name="TmpDat">
								<Item name="ApCode" length=" 2" value="46"/>
								<Item name="OFmtCd" length=" 3" value="999"/>
								<Item name="AgrNo"  length="60" desc="协议"/>
							</PackItem>
							<Break/>
						</Case>
						<Case value="2">
							<PackItem name="TmpDat">
								<Item name="ApCode"  length=" 2" value="46"/>
								<Item name="OFmtCd"  length=" 3" value="999"/>
								<Item name="OSBusTy" length=" 5" desc="业务类型"/>
								<Item name="OActFlg" length=" 1" desc="账户性质"/>
								<Item name="OPyrAct" length="32" desc="付款人帐号"/>
								<Item name="OPyrNam" length="60" desc="付款人账户"/>
								<Item name="OPyrOpn" length="12" desc="付款人开户行"/>
								<Item name="OUniCod" length="20" desc="收款单位编码"/>
								<Item name="OPyeAct" length="32" desc="收款人帐号"/>
								<Item name="OPyeNam" length="60" desc="收款人账户"/>
								<Item name="OPyeOpn" length="12" desc="收款人开户行"/>
								<Item name="OSmr"    length="60" desc="备注"/>
								<Item name="OTCusId" length=" 8" desc="借用表中该字段存放存折凭证号码"/>
							</PackItem>
							<Break/>
						</Case>
					</Switch>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="W"/>
				<Case value="E">
					<PackItem name="TMP">
						<Item name="ApCode" length="2" value="SC"/> 
						<Item name="OFmtCd" length="3" value="D04"/>
						<Item name="RspCod" length="6"/>
						<Item name="InPos"  length="4" value="0001"/>
						<Item name="RspMsg" length="56"/>
					</PackItem>
					<Item name="TMP" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Case value="A">
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Case>
			</Switch>
		</Macro>
<!--定期借记业务协议浏览-->
