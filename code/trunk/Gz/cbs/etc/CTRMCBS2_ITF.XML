									<Item name="TmpNam" head_len="3"/>
								</Group>
							</PackItem>
							<Break/>
						</Case>
						<Case value="2">   <!--单笔查询-->
							<PackItem name="TmpDat">
								<Item name="ApCode" length=" 2" value="46"/>
								<Item name="OFmtCd" length=" 3" value="999"/>
								<Item name="SBusTy" length=" 5" desc="业务类型"/>
								<Item name="ActFlg" length=" 1" desc="账户性质"/>
								<Item name="PyrAct" length="32" desc="付款人帐号"/>
								<Item name="PyrNam" length="60" desc="付款人账户"/>
								<Item name="PyrOpn" length="12" desc="付款人开户行"/>
								<Item name="UniCod" length="20" desc="收款单位编码"/>
								<Item name="PyeAct" length="32" desc="收款人帐号"/>
								<Item name="PyeNam" length="60" desc="收款人账户"/>
								<Item name="PyeOpn" length="12" desc="收款人开户行"/>
								<Item name="Smr"    length="60" desc="备注"/>
								<Item name="TCusId" length=" 8" desc="借用表中该字段存放存折凭证号码"/>
