	<Transaction service="465816" code="465816" timeout="30" log_switch='0' desc="网银往帐状态同步">
		<Request>
			<Include file="etc/TRMTIA_IIT.XML"/>
			<If condition="IS_EQUAL_STRING($TIATyp,P)">
				<Item name="PagIdx"   length="6"/>
				<AddItem name="Func" value="1"/>
			</If>
			<Else>
				<Item name="Func" length="1" desc="功能"/>
				<Switch expression="$Func">
					<Case value="1">	<!--浏览-->
						<Item name="BWebChv" length="8" desc="网银起始流水"/>
						<Item name="EWebChv" length="8" desc="网银结束流水"/>
						<Break/>
					</Case>
					<Case value="2"/>	<!--单笔查询-->
					<Case value="3">	<!--单笔同步-->
						<Item name="OLogNo" length="14" desc="前置流水号"/>
						<Break/>
					</Case>
				</Switch>
			</Else>
		</Request>
		<Response necessary="no">
			<Include file="etc/TRMTOA_IIT.XML"/>
			<Switch expression="$MsgTyp">
				<Case value="N">
					<Switch expression="$Func">
						<Case value="1">   <!--浏览查询-->
							<PackItem name="TmpDat">
								<Item name="ApCode"  length="2" value="32"/>
								<Item name="OFmtCd"  length="3" value="Z01"/>
								<Item name="PageNo"  length="4" value="0001"/>
								<Item name="VarSize" length="1" value="3"/>
								<Item name="Ttl"     head_len="3" value="网银转发流水浏览"/>
								<Item name="SubTtl"  head_len="3" value="浏览内容"/>
								<Group name="Rec" repeat_name="RecNum">
									<PackItem name="TmpNam">
										<SwitchCode name="DtlSts" tab_name="DtlSts" col_name="DtlSts"/>
										<Item name="DtlSts" length=" 8" desc="状态"/>
										<Item name="LogNo"  length="14" desc="前置流水号"/>
										<Item name="ActDat" length=" 8" desc="转发日期"/>
										<Item name="WebChv" length=" 8" desc="网银流水号"/>
										<Item name="DrAct"  length="21" desc="借方账号"/>
										<Item name="TxnAmt" length="15" desc="交易金额"/>
									</PackItem>
									<Item name="TmpNam" head_len="3"/>
								</Group>
							</PackItem>
							<Break/>
						</Case>
						<Case value="2">   <!--单笔查询-->
							<PackItem name="TmpDat">
								<Item name="ApCode" length=" 2" value="46"/>
								<Item name="OFmtCd" length=" 3" value="999"/>
								<Item name="DtlSts" length=" 1" desc="明细状态"/>
								<Item name="OLogNo" length="14" desc="前置流水号"/>
								<Item name="ActDat" length=" 8" desc="转发日期"/>
								<Item name="TlrId"  length=" 7" desc="柜员号"/>
								<Item name="TckNo"  length="11" desc="会计流水"/>
								<Item name="HRspCd" length=" 6" desc="主机返回码"/>
								<Item name="WebChv" length=" 8" desc="网银流水号"/>
								<Item name="RecNo"  length="14" desc="系统流水号"/>
								<Item name="DrAct"  length="21" desc="借方账号"/>
								<Item name="DrNam"  length="60" desc="借方户名"/>
								<Item name="CrAct"  length="32" desc="贷方账号"/>
								<Item name="CrNam"  length="60" desc="贷方户名"/>
								<Item name="TxnAmt" length="15" desc="交易金额"/>
								<Item name="Rmk"    length="60" desc="摘要"/>
								<Item name="RvsNo"  length="12" desc="销账编号"/>
								<Item name="Msg"    length="60" desc="附言"/>
								<Item name="OppExc" length="10" desc="对方交换号"/>
								<Item name="RcvBNo" length="12" desc="接收行号"/>
								<Item name="FeeAmt" length="15" desc="手续费"/>
								<Item name="PstAmt" length="15" desc="邮电费"/>
							</PackItem>
							<Break/>
						</Case>
						<Default>
							<Item name="BillText" deli_asc="-1"/>
							<Break/>
						</Default>
					</Switch>
					<If condition="OR(IS_EQUAL_STRING($Func,1),IS_EQUAL_STRING($Func,2))">
						<Item name="TmpDat" head_len="4" include_len="yes"/>
					</If>
					<Break/>
				</Case>
				<Case value="E">
					<PackItem name="TmpDat">
						<Item name="ApCode" length="2" value="SC"  />
						<Item name="OFmtCd" length="3" value="D04" />
						<Item name="RspCod" length="6" />
						<Item name="InPos"  length="4" value="0001" />
						<Item name="RspMsg" length="56" />
					</PackItem>
					<Item name="TmpDat" head_len="4" include_len="yes"/>
					<Break/>
				</Case>
				<Default>
					<Item name="BillText" deli_asc="-1"/>
					<Break/>
				</Default>
			</Switch>
		</Response>
	</Transaction>



