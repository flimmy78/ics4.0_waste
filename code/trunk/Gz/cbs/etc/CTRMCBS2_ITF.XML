			<Item name="UsgCod" length=" 3" desc="资金用途代码"/>
			<Item name="TBusTp" length=" 5" desc="业务类型"/>
			<Item name="SndAct" length="35" expression="DELSPACE($SndAct,all)" desc="发起人帐号"/>
			<Item name="SndNm"  length="60" expression="DELSPACE($SndNm,all)"  desc="发起人名称"/>
