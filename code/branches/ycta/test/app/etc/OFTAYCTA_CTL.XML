     </Else>

      <Quote name="TxnSrc_TxnCnl"/>

      <!-- ReadModuleCfg会直接在ConfigDeclare定义的ParaFile文件里面匹配对应的项 -->
      <Exec func="PUB:ReadModuleCfg">
        <Arg name="Application" value="GZ_YCTA"/><!--可以使用单位编号-->
        <Arg name="Transaction" value="484512"/><!--可以用交易码或者模块名-->
      </Exec>

      <!-- 第三方报文字段添加 -->
