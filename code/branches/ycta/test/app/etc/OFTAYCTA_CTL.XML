        <Set>RspCod=EFE999</Set>
        <Set>RspMsg=修改交易明细为已清算失败</Set>
        <Return/>
      </If>  

      <Set>MsgTyp=N</Set>
      <Set>RspCod=000000</Set>
      <Set>RspMsg=交易成功</Set>
    </FlowCtrl>
