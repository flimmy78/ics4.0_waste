      <!--取前置流水号-->
      <Exec func="PUB:GetLogNo"/>
      <!--默认交易返回成功-->
      <Set>RspCod=000000</Set>



     <Exec func="PUB:AddAuthReason" >
         <Arg name="AthCod" value="40" />
         <Arg name="AthMsg" value="YCT000" />
     </Exec>
     <Exec  func="PUB:CheckLocalAuth" >
         <Arg name="ObjSvr" value="SHSTPUB1" />
     </Exec>

