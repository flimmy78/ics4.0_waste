        <Exec func="PUB:ReadRecord">
          <Arg name="SqlCmd" value="TotalDeal_DL"/>
        </Exec>         
        <!-- 由于电力要求无记录也要发送对账文件,因此记录为零判断注释掉,qm,20120503 -->
