<?xml version='1.0' encoding='ISO-8859-1'?>
<BATCH debug_file="agtfmta.log" err_file="agtfmta.err">

  <!--CodeSwitching>
    <Table name="H2T_EFE_441"  file="etc/EFEA_CSW2.XML">
      <Column name="HRspCd" sort="no"/>
      <Column name="TRspCd" sort="no"/>
    </Table>
  </CodeSwitching-->

  <Process name="YCTA_Dtl"   desc="羊城通交易明细">
    <Data>
      <Item name="TERMID"  length="13" align_mode="right" />   <!-- 供电公司系统参考号 -->
      <!--Item name="Flag" length="1" value="	" /-->
      <Item name="SYS_SEQ" length="17" align_mode="right" />   <!-- 供电公司用户编号 -->
      <Item name="ICN"     length="17" align_mode="right" />   <!-- 供电公司清算日期 -->
      <Item name="TIM"     length="15" align_mode="right" />   <!-- 银行交易日期 -->
      <Item name="TF"      length="9"  align_mode="right" /> <!-- 银行交易时间 -->
      <Item name="BAL"     length="9"  align_mode="right" /> <!-- 银行交易流水号 -->
      <Item name="TT"      length="2"  align_mode="right" /> <!--客户帐号-->
    </Data>
  </Process>    

</BATCH>
